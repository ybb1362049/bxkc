	var cid = getQueryString("cataId");
	if(cid==null){
		window.location.href = "/website/zyjyzx/html/index.html";
	}

	/********   begin 自动滚屏   *******/
	var currentpos,timer;
	function scrollinitialize(){
		timer=setInterval("scrollwindow()",50);
	}
	function scrollstop(){
		clearInterval(timer);
	}
	function scrollwindow(){
		currentpos=document.body.scrollTop;
		window.scroll(0,++currentpos);
		if (currentpos != document.body.scrollTop)
			scrollstop();
	}
	document.onmousedown=scrollstop;
	document.ondblclick=scrollinitialize;
	/********   end 自动滚屏   *******/
</script>
<script type="text/javascript">
function searchForm(){
	try{
		var key = $("#searchKey").val();
		if(key.trim()==""){
			alert("请输入关键字搜索！");
			$("#searchKey").focus();
			return;
		}
		var url = "/website/zyjyzx/html/search.html?key=" + escape(key)+"&tmp=" + Math.random();;
		window.open(url);
	}
	catch(e){}
}
</script>
