<script type="text/javascript">
function searchForm(){
	try{
		var key = $("#searchKey").val();
		if(key.trim()==""){
			alert("请输入关键字搜索！");
			$("#searchKey").focus();
			return;
		}
		var url = "/website/zyjyzx/html/search.html?key=" + escape(key)+"&tmp=" + Math.random();;
		window.open(url);
	}
	catch(e){}
}
</script>
