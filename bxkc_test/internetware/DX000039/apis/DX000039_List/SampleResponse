<script>
function ChangClass(num)
{   
	for(i=1; i<7; i++)
	{
	  if (i==num )
	  { 
	  	  document.getElementById("sub_"+i+"").style.display="block";
	  }
	  else
	  {
		  document.getElementById("sub_"+i+"").style.display="none";
	  }
	} 
}
</script>
