        $(".tabs_1").each(function (index) {
            $(this).click(function () {
                $(".tabs_1").removeClass("tab1_alr");
                $(".tabs_1:eq(" + index + ")").addClass("tab1_alr");
                $(".tab1_cont").css("display", "none");
                $(".tab1_cont:eq(" + index + ")").css("display", "block");
            });
        });
    });
    $(function () {
        $(".tabs_2").each(function (index) {
            $(this).click(function () {
                $(".tabs_2").removeClass("tab2_alr");
                $(".tabs_2:eq(" + index + ")").addClass("tab2_alr");
                $(".tab2_cont").css("display", "none");
                $(".tab2_cont:eq(" + index + ")").css("display", "block");
            });
        });
    });
    $(function () {
        $(".tabs_3").each(function (index) {
            $(this).click(function () {
                $(".tabs_3").removeClass("tab3_alr");
                $(".tabs_3:eq(" + index + ")").addClass("tab3_alr");
                $(".tab3_cont").css("display", "none");
                $(".tab3_cont:eq(" + index + ")").css("display", "block");
            });
        });
    });
    </script>
	
	
</body>
</html>