<h3><a onclick="$('#__jc_form__').toggle();" href="javascript:void(0)" style="color:red;float:right;">我来完善</a></h3><div class="clear"></div>
<form method="post" onsubmit="return chkJcSub();" action="/app_nxqw/guestbook/jcxx/save.jhtml" target="_blank" style="display: none;" id="__jc_form__">
			<script>
			function chkJcSub(){
	         	var userName= $("#userName").val();
	         	var userPhone= $("#userPhone").val();
	         	var title= $("#title").val();
	         	var content= $("#content").val();
	         	if(title.length&lt;1){
	         		alert("标题不能为空");
	         		return false;
	         	}
	         	if(userName.length&lt;1){
	         		alert("姓名不能为空");
	         		return false;
	         	}else if(userName.length&gt;15){
	         		alert("姓名长度超出限制");
	         		return false;
	         	}
	         	if(userPhone.length&lt;1){
	         		alert("电话不能为空");
	         		return false;
	         	}else if(isNaN(userPhone)){
	         		alert("电话只能是数字");
	         		return false;
	         	}else if(userPhone.length&gt;15){
	         		alert("电话长度超出限制");
	         		return false;
	         	}
	         	
	         	if(content.length&lt;1){
	         		alert("内容不能为空");
	         		return false;
	         	}
	         	return true;
	          }   
			</script>
			
			
