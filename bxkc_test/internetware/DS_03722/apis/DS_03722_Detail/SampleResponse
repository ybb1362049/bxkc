<script language="javascript" type="text/javascript">
function native2ascii(strNative) {
	//alert(strNative+'======');
    var output = "";
	if(strNative && ""!=strNative ){
		for (var i=0; i<strNative.length; i++) {		
			var c = strNative.charAt(i);	
			var cc = strNative.charCodeAt(i);
			if (cc > 0xff){	
				output += "\\u" + toHex(cc >> 8) + toHex(cc & 0xff);
			}else{				
				output += c;
			}		
		}
	}
    return output;
}


$(function(){

	$("#searchNewsBtn").click(function(){
	   $("#input01").val($("#input01").val());
		var keyValue = native2ascii($("#input01").val());
		window.open("/CmsNewsController.do?method=search&chnlCodes="+$("#chnlCodes").val()+"&searchKey="+keyValue+"&pageSize=10&searchResultForm=search_result.ftl")
	})
	
	$("p[name=hotSearch] a").each(function(i,n){
		var title=n.title+"";
		$("p[name=hotSearch] a[title="+title+"]").live("click",function() {
			var titleValue = native2ascii(title);
			window.open("/CmsNewsController.do?method=search&chnlCodes="+titleValue+"&pageSize=10&searchResultForm=search_result.ftl")
		});
		
	});
})
</script>
