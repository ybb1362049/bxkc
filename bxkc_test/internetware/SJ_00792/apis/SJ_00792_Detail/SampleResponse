    <script>
        $(function () {
            juicer.register('formatDate', formatDate);
            //模块鼠标经过效果
            $(".safeItem,.softCom,.agencyLi").hover(function () {
                $(this).addClass("on");
            }, function () {
                $(this).removeClass("on");
            });
            $("#LoginOut").on("click", function () {
                AjaxJson("/Login/LoginOut", {}, function (data) {
                    layer.msg("您已退出登录，欢迎再来哦 ^_^", { icon: 6, shade: [0.3, '#000'] }, function () {
                        location.href = "/Home/Index";
                    });
                });
            });
            //设置导航高亮
            setTopNavLight()
        });
        //设置导航高亮函数
        function setTopNavLight() {
            var li = $(".w_header .mAuto li");
            li.removeClass("on");

            var index = "";
            li.each(function (i, e) {
                var liNow = $(e);
                if (i + 1 == index) {
                    liNow.addClass("on");
                    return false;
                }
            });
        }

        function building(btn) {
            //layer.msg("亲，该栏目正在建设中，请静候佳音~", { icon: 6 });
            layer.tips('亲，栏目正在筹备中，请静候佳音~', $(btn), {
                tips: [1, '#3595CC']
            });
        }
    </script>
    
