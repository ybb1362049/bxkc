								function submitCheck(sform){
									   var fullText = $(sform).find("#fullText").val();
									   var placeholder = $(sform).find("#fullText").attr("placeholder");//解决ie8兼容性问题
									   if(fullText=="" || fullText == placeholder){
										    return false;
									   }else{
									   	  return true;
									   }
								    }
