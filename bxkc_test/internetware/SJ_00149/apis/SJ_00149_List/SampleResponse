(function () {
var num = $("#jsNav_8666 li").length;
$("#play_8666 ul li:first").css("display", "block");
$("#jsNav_8666 li:first").addClass("over");
var curr = 0;
$("#jsNav_8666 li").each(function (i) {
$(this).click(function () {
curr = i;
$("#play_8666 ul li").eq(i).css("z-index", "10").siblings("li").css("z-index", "1");
$("#play_8666 ul li").eq(i).fadeIn(1500).siblings("li").fadeOut(1500);
$(this).siblings("li").removeClass("over").end().addClass("over");
return false;
});
});
//自动翻
var timer = setInterval(function () {
todo = (curr + 1) % num;
$("#jsNav_8666 li").eq(todo).click();
}, 9000);

//鼠标悬停在触发器上时停止自动翻
$("#jsNav_8666 li").hover(
function () {
clearInterval(timer);
},
function () {
timer = setInterval(function () {
todo = (curr + 1) % num;
$("#jsNav_8666 li").eq(todo).click();
}, 5000);
}
);
})();
</script></div>
