        window.onload = function () {

            var obj_select = document.getElementById("typeOne");
            var obj_content = document.getElementById("typeTwo").getElementsByTagName('input');

            obj_select.onchange = function () {

                if (obj_select.value == 1) {
                    obj_content.item(0).style.display = 'block';
                    obj_content.item(1).style.display = 'none';
                    obj_content.item(2).style.display = 'none';
                }
                if (obj_select.value == 2) {
                    obj_content.item(0).style.display = 'none';
                    obj_content.item(1).style.display = 'block';
                    obj_content.item(2).style.display = 'none';
                }
                if (obj_select.value == 3) {
                    obj_content.item(0).style.display = 'none';
                    obj_content.item(1).style.display = 'none';
                    obj_content.item(2).style.display = 'block';
                }

            }

        }

    </script>
    <script>

        function upPage() {

            var pageNo = $("#pageNo").val();

            var menuKey = $("#menuKey").val();

            if (pageNo - 1 < 1) {

                alert("已经是第一页");

            } else {

                initInfos("pageList", menuKey, pageNo - 1);

            }

        }

        function downPage() {

            var pageNo = $("#pageNo").val();

            var menuKey = $("#menuKey").val();

            initInfos("pageList", menuKey, parseInt(pageNo) + 1);

        }

        /**
         ajax二级联动
         **/
        function getCity(val) {
            $.ajax({
                type: "post",
                url: '/plist/getCity',
                data: {"proId": val},
                dataType: 'json',
                success: function (data) {
                    if (data.isok == "T") {
                        var citys = data.citys;
                        var html = "<option>选择地区</option>";
                        for (var i = 0; i < citys.length; i++) {
                            html += "<option value='" + citys[i].id + "'>" + citys[i].strvalue + "</option>";
                        }
                        $("#city").html(html);
                    } else
                        alert(data.errmsg);
                }
            });
        }
        $(function () {
            if (102==102 || 102== 105
            )
            {//typeOne
                var pro = "";
                if (pro != null && pro != "") {
                    $("#province").val(pro);
                    //同时将城市给显示出来
                    var html = "<option>--请选择--</option>";
                    var cs = '[]';
                    var citys = JSON.parse(cs);
                    if (citys != null && citys != '') {
                        for (var i = 0; i < citys.length; i++) {
                            html += "<option value='" + citys[i].id + "'>" + citys[i].strvalue + "</option>";
                        }
                        $("#city").html(html);
                        var city = "";
                        if (city != null && city != '') {
                            $("#city").val(city);
                        }
                    }
                }
                var ptone = "";
                if (ptone != null && ptone != "") {
                    $("#typeOne").val(ptone);
                    var pttwo = '';
                    var ptwos = JSON.parse(pttwo);
                    if (ptwos != null && ptwos != '') {
                        var ptwo = ", ";
                        if (ptwo != null && ptwo != '') {
                            var html = "    ";
                            for (var i = 0; i < ptwos.length; i++) {
                                var ck = "";
                                if (-1 != ptwo.indexOf(ptwos[i].bkey)) {
                                    ck = "checked";
                                }
                                html += "<input type='checkbox' name='ptpTwo' " + ck + " value='" + ptwos[i].bkey + "'  /> " + ptwos[i].strvalue + "  ";
                            }
                            $("#typeTwo").html(html);
                        } else {
                            var html = "    ";
                            for (var i = 0; i < ptwos.length; i++) {
                                html += "<input type='checkbox' name='ptpTwo' value='" + ptwos[i].bkey + "'  /> " + ptwos[i].strvalue + "  ";
                            }
                            $("#typeTwo").html(html);
                        }
                    }
                }
            }
        });
        //品类联动
        function getTypeTwo(val) {
            if (val == null || val == '') {
                $("#typeTwo").html("");
            } else {
                var rs = val.split("@@")[0];
                $.ajax({
                    type: "post",
                    url: '/plist/getTypeTwo',
                    data: {"proId": rs},
                    dataType: 'json',
                    success: function (data) {
                        if (data.isok == "T") {
                            var citys = data.citys;
                            var html = "    ";
                            for (var i = 0; i < citys.length; i++) {
                                html += "<input type='checkbox' name='ptpTwo' value='" + citys[i].bkey + "'  /> " + citys[i].strvalue + "  ";
                            }
                            $("#typeTwo").html(html);
                        } else
                            alert(data.errmsg);
                    }
                });
            }
        }
        function search() {

            $("#subForm").submit();
        }
    </script>
