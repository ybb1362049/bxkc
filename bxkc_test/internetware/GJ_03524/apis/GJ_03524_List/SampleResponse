            <script type="text/javascript">

                function topage(obj) {

                    //取消其他的选中效果
                    $(".menuDiv ul li").removeClass("sidebar_active");
                    //修改文章列表标题
                    var pageTitle = $(obj).html();
                    $("#pageTitle").html(pageTitle);
                    //选中效果
                    $(obj).find("a").attr("class", "sidebar_active");
                    var menuKey = $(obj).attr("menuKey");
                    //赋值
                    //$("#menuKey").val(menuKey);
                    //加载数据
                    window.location.href = "/plist/" + menuKey;
                    //initInfos('pageList',menuKey,1);

                }

                $(function () {
                    var menuKey = $("#menuKey").val();
                    $("li[menuKey='caigou']").find("a").attr("class", "sidebar_active");
                    //修改文章列表标题
                    var pageTitle = $("li[menuKey='" + menuKey + "']").html();
                    $("#pageTitle").html(pageTitle);
                    $(".menuDiv ul").hide();
                    var pageType = "111";
                    $("ul[ulType='" + pageType + "']").show();
                    $(".menuDiv ul li").click(function () {
                        topage(this);
                    });

                });

            </script>

