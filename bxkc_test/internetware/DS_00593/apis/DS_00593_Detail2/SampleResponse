    <script type="text/javascript">
        var uploadType = 0; //上传文件类型
        var sub_System_Guid = "D73A5F08-CAF4-49EB-9BD8-F6BC9439BD09";
        var fileRootUrl = "http://ggzy.yazw.gov.cn:8011/";//上传根目录
        var ZhongJie_JiGou_Guid = "";
        var downCheckUrl = fileRootUrl + "WJGL/FileDownLoadCheckRemote?user_Guid=";//下载检查url
        var uploadUrl = fileRootUrl + "?user_Guid=&user_Name=&sub_System_Guid=D73A5F08-CAF4-49EB-9BD8-F6BC9439BD09";//上传的url
        var AttacthFileRootUrl = "http://ggzy.yazw.gov.cn:8011/UploadFiles/";
        var CaiGouXiTongUrl = "http://ggzy.yazw.gov.cn:8017/";

        //登陆
        function login() {
            window.location = CaiGouXiTongUrl;
        }

        //当前登录人
        $(function () {
            $("#user_quit").hide();
            //获取当前登陆人
            getLoginZJJGRemote(function (ZJJG) {
                if (ZJJG == null) {  
                    $("#user_regist").show();
                    $("#user_quit").hide();
                }
                else {
                    //$("#user_regist").hide();
                    //$("#user_quit").show();                   
                    //$("#user_name").text(ZJJG.ZhongJie_JiGou_Name, 1);
                    //if (ZJJG.ZhongJie_JiGou_Type == "1") {
                    //    ZhongJie_JiGou_Guid = ZJJG.ZhongJie_JiGou_Guid;
                    //}
                }
            });           
        });


        function Search()
        {          
        	if ($("#s").val()=="")
        	{
        		alert("请输入搜索的关键字！");
        		return;
        	}
            window.open('/Home/Search?key=' + $("#s").val());
        }


        //获取登陆的供应商 远程
        function getLoginZJJGRemote(recall) {
            $.ajax({
                type: 'post',
                dataType: 'jsonp',
                jsonp: 'jsonpCallback',
                async: false,
                url: CaiGouXiTongUrl + "Home/getLoginZJJGRemote",
                success: function (rData) {
                    if (rData.success) {
                        if (recall != undefined) {
                            recall(rData.entity);
                        }
                    }
                    else {
                        recall(null);
                    }
                },
                error: function (xhr, status, error) {
                    //alert(xhr.responseText);
                }
            });
        }

        //获取登陆的供应商 远程
        function quit() {
            
            if(confirm('确认要退出雅安市中介超市吗？') == true)
            {
                    // 退出操作;
                    $.ajax({
                        type: 'post',
                        dataType: 'jsonp',
                        jsonp: 'jsonpCallback',
                        async: false,
                        url: CaiGouXiTongUrl + "Home/LogoutRemote",
                        success: function (rData) {
                            if (rData.success) {
                                location.reload();
                            }
                            else {
                                alert(rData.msg);
                            }
                        },
                        error: function (xhr, status, error) {
                            //alert(xhr.responseText);
                        }
                    });
             }            
        }


        var preSelected = $("#lihead")[0];
        function showTitleStyule(TitleId) {
            if (preSelected) {
                preSelected.className = "";
            }
            var title = $("#" + TitleId)[0];
            preSelected = title;
            if (preSelected == undefined) {
                return;
            }
            preSelected.className = "current";
        }
    </script>
