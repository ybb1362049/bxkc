<script>
    function Ajax_GoPage() {

        var obj = $("#txtAjax_PageIndex");

        var reg = new RegExp("^\\d+$");
        if (!reg.test(obj.val()) || (obj.val() <= 0 || obj.val() > parseInt("3"))) {
            alert("请输入正确的页码！");
            obj.focus();
            obj.select();
            return;
        }
        var strUrl = "/JyWeb/XXGK/GetXXGKList?PageSize=15&SubType=30&SubType2=0&Type=%E4%BF%A1%E6%81%AF%E5%85%AC%E5%BC%80" + "&pageIndex=" + obj.val();
        $.ajax({
            url: strUrl,
            type: "post",
            success: function (rData) {
                if ($("#divContent")) {
                    $("#divContent").html(rData);
                }
            }
        });
    }
</script>
