<script type="text/javascript">
    function addFJ() {
        var xxguid = "";
        xxguid = $("#XinXi_Publish_Guid").length>0?$("#XinXi_Publish_Guid").val():"";
        $.ajax({
            url: "/JyWeb/XXGK/GetFJList?guid=" + xxguid,
            type: "post",
            dataType: "json",
            success: function (data) {
                for (var i = 0; i < data.length; i++) {
                    getUploadInfo(data[i].Attach_Guid, data[i].Attach_Client_FileName, data[i].Attach_Server_FileName, "", data[i].Create_Time);
                }
            }
        });
    }

    function getUploadInfo(guid, oldName, newName, clientId, upload_endtime) {
        if (upload_endtime == undefined) {
            upload_endtime = "";
        }
        $("#divFJ").show();
        var strTrHtml = "<tr><td style='width:10%;text-align:right;'><img alt='' src='/Areas/JyWeb/Content/App_Themes/Default/Images/下载按钮2.png' style='padding-top:7px;padding-right:2px;'>附件：</td><td style='line-height:100%;padding-left:0;'><a  style=\"color: blue\" id='downfile" + guid + "' href=\"javascript:downFile('" + guid + "')\">" + oldName + "</a></td></tr>"
        $("#test").append(strTrHtml);
    }
    $("#aBtnSearch").mousemove(function ()
    {
    	$("#aBtnSearch").attr("href", "/JyWeb/XXGK/GetXXGKList" + "?SubType=30&SubType2=0&XinXi_Title=" + escape($("#txtSearch").val()));

    });
	function addHref()
	{
		$("#aBtnSearch").attr("href", "/JyWeb/XXGK/GetXXGKList" + "?SubType=30&SubType2=0&XinXi_Title=" + escape($("#txtSearch").val()));
	}
    $(function () {
        addFJ();
    	
    	$("#aBtnSearch").attr("href", "/JyWeb/XXGK/GetXXGKList" + "?SubType=30&SubType2=0&XinXi_Title=" + escape($("#txtSearch").val()));
    })
</script>
