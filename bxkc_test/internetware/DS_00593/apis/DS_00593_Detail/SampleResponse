        var uploadType = 0; //上传文件类型
        var sub_System_Guid = "D73A5F08-CAF4-49EB-9BD8-F6BC9439BD09";
        var fileRootUrl = "http://ggzy.yazw.gov.cn:8011/";//上传根目录
