    var common_uploader_url = "/NewCommon/Common/GetUploaderHtml?1=1";
    //获取上传信息
    function getUploadInfo(guid, oldName, newName, clientId, upload_endtime) {
        var strTrHtml = "<tr><td style='width:10%;text-align:right;'><img alt='' src='/Areas/JyWeb/Content/App_Themes/Default/Images/下载按钮2.png' style='padding-top:7px;padding-right:2px;'>附件：</td><td style='line-height:100%;padding-left:0;'><a  style=\"color: blue\" id='downfile" + guid + "' href=\"javascript:downFile('" + guid + "')\">" + oldName + "</a></td></tr>"
        $("#test").append(strTrHtml);
    }
    function addFJ() {
        var xxguid = "";
        xxguid = "39a80a79-2370-4a71-af07-da591fc9eaf8";
        $.ajax({
            url: "/JyWeb/XXGK/GetVW_XXGK_FJ_List?guid=" + xxguid,
            type: "post",
            dataType: "json",
            success: function (data) {
                for (var i = 0; i < data.length; i++) {
                    getUploadInfo(data[i].Attach_Guid, data[i].Attach_Client_FileName, data[i].Attach_Server_FileName, "", data[i].Upload_EndTime);
                }
            }
        });
    }

    function Back_OnClick() {
        window.location.href = "/JyWeb/ZJJGXQ/ZJJGXQList?type=80010";
    }
    //当前登录人
    function BaoMing() {
        if (ZhongJie_JiGou_Guid != "") {
           // window.open("http://ggzy.yazw.gov.cn:8017/" + "JYGL/Home/Index?urlType=1&guid=39a80a79-2370-4a71-af07-da591fc9eaf8");
            window.open("http://ggzy.yazw.gov.cn:8017/" + "JYGL/Home/Index?urlType=1&guid=39a80a79-2370-4a71-af07-da591fc9eaf8"+"&BaoMing=1"); 
        } else {
            window.open("http://ggzy.yazw.gov.cn:8017/" + "Login/Login_Index?user_type=-1&urlType=0");
        }
    }

