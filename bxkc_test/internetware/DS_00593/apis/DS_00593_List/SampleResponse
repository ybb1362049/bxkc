<script type="text/javascript">
    

    $("#YiJi").change(function () {
        yijiChange();
        getSearchList();
    })
    function yijiChange()
    {
        $.ajax({
            type: 'post',
            url: '/JyWeb/ZJJGXQ/GetErJiList?YiJiGuid=' + $("#YiJi").val(),
            async:false,
            dataType: 'json',
            success: function (data)
            {
                if (data.length > 0) {
                    $("#ErJi").show();
                    var erji = "<option value='请选择' selected='selected'>请选择</option>"
                    for (var i = 0; i < data.length; i++) {
                        erji += "<option value='" + data[i].ZiZhi_LeiBie_Guid + "'>" + data[i].ZiZhi_LeiBie_Name + "</option>";
                    }
                    $("#ErJi").html(erji);
                }
                else {
                    $("#ErJi").html("");
                    $("#ErJi").hide();
                    $("#SanJi").html("");
                    $("#SanJi").hide();
                }                
            }
        });
    }



    $("#ErJi").change(function () {
        erjiChange();
        getSearchList();
    })

    function erjiChange() {
        $.ajax({
            type: 'post',
            url: '/JyWeb/ZJJGXQ/GetSanJiList?ErJiGuid=' + $("#ErJi").val(),
            dataType: 'json',
            async: false,
            success: function (data) {
            	if (data.length > 0)
            	{
                    $("#SanJi").show();
                    var erji = "<option value='' selected='selected'>请选择</option>"
                    for (var i = 0; i < data.length; i++) {
                        erji += "<option value='" + data[i].ZiZhi_LeiBie_Guid + "'>" + data[i].ZiZhi_LeiBie_Name + "</option>";
                    }
                    $("#SanJi").html(erji);
                }
                else {
                    $("#SanJi").html("");
                    $("#SanJi").hide();
                }                
            }
        });
    }

    $("#SanJi").change(function () {        
        getSearchList();
    })



    $("#aBtnSearch").mousemove(function ()
    {
    	var YiJi = $("#YiJi").val();
    	var ErJi = $("#ErJi").val();
    	var SanJi = $("#SanJi").val();
    	if (YiJi == "请选择")
    	{
    		YiJi = null
    	}
    	if (ErJi == "请选择")
    	{
    		ErJi = null
    	}
    	if (SanJi == "请选择")
    	{
    		SanJi = null
    	}
    	$("#aBtnSearch").attr("href", "/JyWeb/ZJJGXQ/GetZJJGXQList" + "?type=80010&XinXi_Title=" + escape($("#txtSearch").val()) + "&YiJi=" + YiJi + "&ErJi=" + ErJi + "&SanJi=" + SanJi);
    });
	function getSearchList()
	{
    	var YiJi = $("#YiJi").val();
    	var ErJi = $("#ErJi").val();
    	var SanJi = $("#SanJi").val();
    	if (YiJi == "请选择")
    	{
    		YiJi = null
    	}
    	if (ErJi == "请选择")
    	{
    		ErJi = null
    	}
    	if (SanJi == "请选择")
    	{
    		SanJi = null
    	}
    	$("#aBtnSearch").attr("href", "/JyWeb/ZJJGXQ/GetZJJGXQList" + "?type=80010&XinXi_Title=" + escape($("#txtSearch").val()) + "&YiJi=" + YiJi + "&ErJi=" + ErJi + "&SanJi=" + SanJi);
		$("#spanBtn").click();
    }
	function addHref()
	{
		var YiJi = $("#YiJi").val();
		var ErJi = $("#ErJi").val();
		var SanJi = $("#SanJi").val();
		if (YiJi == "请选择")
		{
			YiJi = null
		}
		if (ErJi == "请选择")
		{
			ErJi = null
		}
		if (SanJi == "请选择")
		{
			SanJi = null
		}
		$("#aBtnSearch").attr("href", "/JyWeb/ZJJGXQ/GetZJJGXQList" + "?type=80010&XinXi_Title=" + escape($("#txtSearch").val()) + "&YiJi=" + YiJi + "&ErJi=" + ErJi + "&SanJi=" + SanJi);
	}

	$(function ()
	{
		//设置导航栏样式
		showTitleStyule("a中介机构选取");
		var YiJi = '';
		var ErJi = '';
		var SanJi = '';
		if (YiJi == "请选择")
		{
			YiJi = null
		}
		if (YiJi != "" && YiJi != null)
		{
			$("#YiJi").val(YiJi);
			yijiChange();
			// $("#YiJi").trigger("change");
		}
		if (ErJi != "null" && ErJi != null && ErJi != '')
		{
			$("#ErJi").show();
			$("#ErJi").val(ErJi);
			// $("#ErJi").trigger("change");
			erjiChange();
		}
		if (SanJi != "null" && SanJi != null && SanJi != '')
		{
			$("#SanJi").show();
			$("#SanJi").val(SanJi);
			//$("#SanJi").trigger("change");
		}
		$("#aBtnSearch").attr("href", "/JyWeb/ZJJGXQ/GetZJJGXQList" + "?type=80010&XinXi_Title=" + escape($("#txtSearch").val()) + "&YiJi=" +  YiJi + "&ErJi=" + ErJi + "&SanJi=" + SanJi);
	});
</script>
