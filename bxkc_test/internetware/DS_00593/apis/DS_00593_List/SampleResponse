<script>
    function Ajax_GoPage() {

        var obj = $("#txtAjax_PageIndex");

        var reg = new RegExp("^\\d+$");
        if (!reg.test(obj.val()) || (obj.val() <= 0 || obj.val() > parseInt("137"))) {
            alert("请输入正确的页码！");
            obj.focus();
            obj.select();
            return;
        }
        var strUrl = "/JyWeb/ZJJGXQ/GetZJJGXQList?PageSize=15&type=80010" + "&pageIndex=" + obj.val();
        $.ajax({
            url: strUrl,
            type: "post",
            success: function (rData) {
                if ($("#divContent")) {
                    $("#divContent").html(rData);
                }
            }
        });
    }
</script>
