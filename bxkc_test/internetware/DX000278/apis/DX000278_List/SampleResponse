      <script type="text/javascript">
         function doShowMoreOpenRequest(pageNo) {
            gtQuery("#pageNo").val(pageNo);
            document.forms[0].submit();
         }
         
         function pageNoChange() {
            var pageNo = parseInt(gtQuery("#pageNoSelect").val());
            doShowMoreOpenRequest(pageNo);
         }
         
         function initSelectPageNo(pageNo) {
            gtQuery("#pageNoSelect").val(parseInt(pageNo));
         }
         
         function myload() {
            initSelectPageNo(gtQuery("#pageNo").val());
         }
         
         function doChangeLang(lang) {
            var url = webContext + "/open/open.do?method=doChangeLang&language=" + lang;
            document.openForm.action = url;
            document.openForm.submit();
         }
      </script>
