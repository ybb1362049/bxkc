<script type="text/javascript" >
   var _lang = "zh_CN/";
   var webContext = "/8thManage";
   var skinType = "demo";
   var localeFlag = "zh";
   function formatImgSrc(orginalSrc,i18n){
     var index = orginalSrc.indexOf('images');
     if(index>0){
           orginalSrc=orginalSrc.substring(index+6);
     }
     var newImgSrc = webContext+"/images/"+skinType;
     if(false != i18n && (orginalSrc.indexOf("/")<0||orginalSrc.indexOf("\\")<0)){
        newImgSrc=newImgSrc+ '/'+localeFlag;      
     }
     if(orginalSrc.indexOf("/")!= 0||orginalSrc.indexOf("\\")!=0){
        newImgSrc+="/";
     }
     return newImgSrc+orginalSrc;
  }
</script>
