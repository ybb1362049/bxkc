    <script>
        function showLC(tid) {
            for (var i = 2002; i <= 2016; i++) {
                $("#" + i).css("display", "none");
                $("#t" + i).removeClass("fB cDRed");
            }
            $("#" + tid).css("display", "");
            $("#t" + tid).addClass("fB cDRed");
            $("#" + tid).fadeIn(500);
        }
    </script>
