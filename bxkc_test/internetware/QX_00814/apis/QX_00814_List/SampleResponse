<script>
    var myObj=$("#nav_C li");
    myObj.mouseover(function(){
        var oW=$(this).index();
        var CH=$(this).children("a");
        myObj.eq(oW).css("border","none");
        myObj.eq(oW+1).css("border","none");
        CH.css({"color":"red","borderRadius":"5% 5% 0em 0em","backgroundColor":"white"})
    });
    myObj.mouseout(function(){
        var oW=$(this).index();
        var CH=$(this).children("a");
        myObj.eq(oW).css("borderLeft","1px solid black");
        myObj.eq(oW+1).css("borderLeft","1px solid black");
        CH.css({"color":"white","borderRadius":"0em 0em 0em 0em","backgroundColor":"#BD0801"})
    });
    var h=0;
    function headBG(){
        if(h<=4){
            h++;
            $(".headbgpic").eq(h).fadeIn("snow");
            $(".headbgpic").eq(h).siblings(".headbgpic").fadeOut("snow");
        }else if(h==5){
            h=0;
            $(".headbgpic").eq(0).fadeIn("snow");
            $(".headbgpic").eq(0).siblings(".headbgpic").fadeOut("snow");
        }
    }
    var headchange=setInterval("headBG()",6000);
