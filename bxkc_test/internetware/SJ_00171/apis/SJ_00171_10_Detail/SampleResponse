      <script type="text/javascript">
                $(document).ready(function() {
                    //网站访问总次数与当天访问次数统计
                    $.ajax({
                        url: "/web/doSiteCounter.action",
                        type: 'post',
                        cache: false,
                        dataType: 'json',
                        async: false,
                        // 异步
                        timeout: 30000,
                        success: function(data) {
                            if (data.msg != "") {
                                var s = data.msg.split("-");
                                document.getElementById('todayT').innerHTML = s[0];
                                document.getElementById('totalT').innerHTML = s[1];
                            }
                        }
                    });
                });
            </script> 
    </div>
