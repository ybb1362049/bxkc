    function globSearch(){
       var sUrl = "http://dig.lsz.gov.cn/dig/search.action?ty=&w=false&f=&dr=true&p=1&sr=score+desc&fq=&advtime=&advrange=&q=";
				var key = document.getElementById("search1016").value;
				if(key == "" || key == "请输入关键字"){
					alert("关键字不能为空!");
					return false;
				}

				key = encodeURI(key);
				sUrl = sUrl + key + '&ext=siteid:3209969'
				window.open(sUrl);
    }
