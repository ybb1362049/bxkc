<script language="JavaScript" type="text/javascript">
function one(){   
	var a=document.getElementsByTagName("input");
	for(var i=0;i<a.length;i++){
		for( var j=i+1;j<=i+1;j++){
		    var y = "b"+j;
		    var z = "c"+j;
		    
			document.getElementById(y).innerHTML = a[i].value.substring(6,7)+"月";
			document.getElementById(z).innerHTML = a[i].value.substring(8,10);

			}
	}

	  
}; 
$(document).ready(function(){   
	one();
});  
</script>
<script type="text/javascript">
function dianji(){
	var page=document.getElementById("dj").value;
	var start=(page-1)*10;
	var r1=/^[1-9][0-9]$/; 
	var r2=/^[1-9]$/; 
	var r3=/^[1-9][0-9][0-9]$/; 
	if(r1.test(page)|| r2.test(page)||r3.test(page)){
		if(page>0&&page<227){
//			location.href="/information/informationAction.do?method=getInformationByColumnId2&columnId=0301&start="+start+"&page="+page+"";
			submitList('','0101','getInformationByColumnId2',start,page);
		}else{
			alert("输入有误，请输入1到"+226+"之间的数字!");
		}
	}else{
		alert("输入有误，请输入1到"+226+"之间的数字!");
	}
	
}

</script> 
</html>