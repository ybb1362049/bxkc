			        function sousuo(){
			        	var t=document.getElementById("info_title").value;
			        	if(null==t||t==""){
			        		window.open("/systemFile/show.jsp");
			        	}else{
			        		window.open("/index/s?query="+encodeURI(encodeURI(document.getElementById("info_title").value))+"&c=0");
