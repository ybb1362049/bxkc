    <script type="text/javascript">
    //分类组件实现
    function SeparatePage(o) {
    	 var path;
        var currentPage = document.getElementById("pageye").value;
        var pageCount = document.getElementById("pageSize").value;
       
        var iPage = Number(currentPage);
        var mPage = Number(pageCount);
        
        if (o == 'next') {
            iPage++;
        }
        if (o == 'previous') {
            iPage--;
        }
        if (o == 'first') {
            iPage = 1;
        }
        if (o == 'last') {
            iPage = Number(pageCount);
        }
        if (o == 'jumpTo') {
            if (isNaN(document.getElementById("jumpTo").value) ||
			   !(document.getElementById("jumpTo").value.match(/^\d+$/)) ||
			   document.getElementById("jumpTo").value == 0) {
                iPage = 1;
            } else {
                iPage = document.getElementById("jumpTo").value;
                if (iPage > mPage) {
                    iPage = Number(pageCount);
                }
            }
        }
        if (iPage > mPage) {
            iPage = Number(pageCount);
        }
        if (iPage < 1) {
            iPage = 1;
        }
       
        document.getElementById("pageye").value = iPage;
        document.getElementById("jumpTo").value = iPage;
      	var localhref;
      	
      	if(window.location.href.lastIndexOf("?pageye")>-1){
      		localhref=window.location.href+"?pageye=";
        	var path = localhref.substring(0, localhref.indexOf("?pageye="));
           	document.location.href = path+"?pageye=" + iPage + "&pageSize="+mPage+"" ;
      	}else if(window.location.href.lastIndexOf("?")>-1){
        	localhref=window.location.href+"&pageye=";
        	var path = localhref.substring(0, localhref.indexOf("&pageye="));
           	document.location.href = path+"&pageye=" + iPage + "&pageSize="+mPage+"" ;
        }else{
        	document.location.href=window.location.href+"?pageye=" + iPage + "&pageSize="+mPage+"";
        }
        
    }
    </script>
  </body>
