    $(function () {
        $(".nav ul li").hover(function () {
            $(this).addClass("on2");
            $(this).find(".sign").show();
            $(this).find(".navson").stop(true, true).slideDown(300);
        },
         function () {
             $(this).removeClass("on2");
             $(this).find(".sign").hide('slow');
             $(this).find(".navson").stop(true, true).slideUp(300);
         });
    });
