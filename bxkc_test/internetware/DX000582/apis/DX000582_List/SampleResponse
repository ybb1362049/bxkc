<script>
    window.onload = function () {
        var radio2 = 1200 / 230;
        $(".nybanner").height($(window).width() / radio2);
        $(".nybanner2").height($(window).width() / radio2);
        $(window).resize(function () {
            var radio2 = 1200 / 230;
            $(".nybanner").height($(window).width() / radio2);
            $(".nybanner2").height($(window).width() / radio2);
        });

        var div2 = $('.about-con').height();
        document.getElementById('leftbox').style.height = div2 + 58 + "px";
    }
</script>

