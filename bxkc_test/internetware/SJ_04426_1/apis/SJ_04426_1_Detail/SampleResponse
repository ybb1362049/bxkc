<script language='javascript' type="text/javascript">
    var focus_width = 325/*幻灯片新闻图片宽度*/
    var focus_height = 207 /*幻灯片新闻图片高度*/
    var text_height = 18 /*幻灯片新闻文字标题高度*/
    var swf_height = focus_height + text_height
    var pics = '';
    var links = '';
    var texts = '';
    function ati(url, img, title) {
        if (pics != '') {
            pics = pics + "|";
            links = links + "|";
            texts = texts + "|";
        }
        pics = pics + escape(img);
        links = links + escape(url);
        texts = texts + title;
    }
</script>

<script language='javascript' type="text/javascript">

    ati('info.aspx?iid=897&mid=102','UploadPhoto/Image/3.jpg','贵州电视台电视大厦演播大厅');ati('info.aspx?iid=896&mid=102','UploadPhoto/Image/2.jpg','水城钢铁（集团）有限责任公司2号焦炉');ati('info.aspx?iid=895&mid=102','UploadPhoto/Image/1.jpg','贵阳奥体中心');

    document.write('<embed src="flash/pixviewer.swf" wmode="opaque" FlashVars="pics=' + pics + '&links=' + links + '&texts=' + texts + '&borderwidth=' + focus_width + '&borderheight=' + focus_height + '&textheight=' + text_height + '" menu="false" bgcolor="white" quality="high" width="' + focus_width + '" height="' + swf_height + '" allowScriptAccess="sameDomain" type="application/x-shockwave-flash">');
</script>


