		    function openAlert() {
		        var sNum = /^[0-9]$/;
		        var filter = /^\s*([A-Za-z0-9_-]+(\.\w+)*@(\w+\.)+\w{2,3})\s*$/;
		        if (document.form1.LianXiDWMC.value == "") {
		            alert('您没有填写“单位中文全称”。');
		            document.form1.LianXiDWMC.focus();
		            document.all.LianXiDWMC.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.LianXiDWMC.value.length < 4) {
		            alert('您认真填写“单位中文全称”。');
		            document.form1.LianXiDWMC.focus();
		            document.all.LianXiDWMC.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.FaRen.value == "") {
		            alert('您没有填写企业法人。');
		            document.form1.FaRen.focus();
		            document.all.FaRen.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.nsrlb.value == "--请选择--") {
		            document.all.ZhuCeZJ.style.backgroundColor = "#FFFFFF";
		            alert('您没有选择“纳税人分类”！');
		            document.all.nsrlb.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.ZhuCeZJ.value == "") {
		            alert('您没有填写注册资金。');
		            document.form1.ZhuCeZJ.focus();
		            document.all.ZhuCeZJ.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.hidTBS_LB.value == "gys" && document.form1.ddlWZLB1.value == "--请选择--") {
		            document.all.ZhuCeZJ.style.backgroundColor = "#FFFFFF";
		            alert('您没有填写“供应商(一级)分类”！');
		            document.all.ddlWZLB1.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.hidTBS_LB.value == "gys" && document.form1.ddlWZLB2.value == "--请选择--") {
		            document.all.ZhuCeZJ.style.backgroundColor = "#FFFFFF";
		            alert('您没有填写“供应商(二级)分类”！');
		            document.all.ddlWZLB2.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.hidTBS_LB.value == "fbs" && document.form1.ddl_ZY.value == "--请选择--") {
		            document.all.ZhuCeZJ.style.backgroundColor = "#FFFFFF";
		            alert('您没有填写“分包商所属(一级)分类”！');
		            document.all.ddl_ZY.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
                else if (document.form1.LianXiR.value == "") {
		            alert('您没有填写联系人姓名。');
		            document.form1.LianXiR.focus();
		            document.all.LianXiR.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.LianXiRSJ.value == "") {
		            alert('您没有填写“联系手机”。');
		            document.form1.LianXiRSJ.focus();
		            document.all.LianXiRSJ.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.LianXiRSJ.value.length != 11) {
		            alert('请正确填写“联系手机”。');
		            document.form1.LianXiRSJ.focus();
		            document.all.LianXiRSJ.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.LianXiYX.value == "") {
		            alert('您没有填写“电子邮箱”。');
		            document.form1.LianXiYX.focus();
		            document.all.LianXiYX.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (!filter.test(document.form1.LianXiYX.value)) {
		            alert('请正确填写您的电子邮箱！');
		            document.all.LianXiYX.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (document.form1.txtCheckCode.value == "") {
		            alert('请输入验证码！');
		            document.all.txtCheckCode.style.backgroundColor = "#FFFEAC";
		            return false;
		        }
		        else if (!confirm('确认提交您的报名登记信息吗？'))
		            return false;
		    }

		    function isNumber(oNum) {
		        if (!oNum) return false;
		        var strP = /^\d+(\.\d+)?$/;
		        if (!strP.test(oNum)) return false;
		        try {
		            if (parseFloat(oNum) != oNum) return false;
		        }
		        catch (ex) {
		            return false;
		        }
		        return true;
		    }

		    // 光标事件
		    function fEvent(sType, oInput) {
		        switch (sType) {
		            case "focus":
		                oInput.isfocus = true;
		            case "mouseover":
		                oInput.style.borderColor = '#9ecc00';
		                break;
		            case "blur":
		                oInput.isfocus = false;
		            case "mouseout":
		                if (!oInput.isfocus) {
		                    oInput.style.borderColor = '#cccccc'; //84a1bd
		                }
		                break;
		        }
		    }

		    // 得到验证码
		    function getimgcode() {
		        document.all("imgID").src = "../CheckCode.aspx?time=" + Math.random();
		    }
