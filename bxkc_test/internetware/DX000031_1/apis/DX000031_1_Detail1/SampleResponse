	    $(document).ready(function () {
	        $("#nav ul li, #login ul li, .buy-info-content ul li.list, .list-info-content ul li.list, .system-info-content ul li.list, .sell-info-content ul li.list, .download-info-content ul li.list").hover(function () {
	            $(this).addClass('active').siblings().removeClass('active');
	        }); //栏目列表切换
	        $("#side-nav ul li").bind({ "mouseover": function () {
	            $(this).addClass('side-hover');
	            $(this).children('a').children('p').addClass('side-animate');
	        }, "mouseleave": function () {
	            $(this).removeClass('side-hover');
	            $(this).children('a').children('p').removeClass('side-animate');
	        }
	        })//右侧导航
	        nav_banner_slide('banner', 4000); //轮播图
	        $.divselect("#divselect", "#inputselect"); //底部多选框
	    })
	</script>
