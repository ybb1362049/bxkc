        <script language="javascript" type="text/javascript">
        var xmlhttp;
        //实例化XMLHttpRequest对象
        function InstantiateXMLHttp() {
            //Mozilla 浏览器
            if (window.XMLHttpRequest) {
                xmlhttp = new XMLHttpRequest();
                //设置MiME类别
                if (xmlhttp.overrideMimeType) {
                    xmlhttp.overrideMimeType('text/xml');
                }
            }
            //IE浏览器
            else if (window.ActiveXObject) {
                //IE 7、8
                try {
                    xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
                }
                //IE 6
                catch (e) {
                    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
                }
            }

            Request_1Ji();
        }

        function Request_1Ji() {
            select_ddlWZLB1 = document.getElementById("ddlWZLB1"); 
            select_ddlWZLB1.options.length = 0; //请空下拉列表框
            xmlhttp.onreadystatechange = Load_1Ji; //设置当XMLHttpRequest对象的状态改变时调用的方法
            xmlhttp.open("GET", "../OutRegister/DealRequestServices.ashx?requesttypeid=1", true); //设置XMLHttpRequest对象请求的方式、地址和是否异步
            xmlhttp.send(null); //发送请求
        }
        function Load_1Ji() {
            select_ddlWZLB1 = document.getElementById("ddlWZLB1"); 
            if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
                var provinces = xmlhttp.responseText.split("|");
                for (var i = 0; i < provinces.length; i++) {
                    var pro = provinces[i].split(";");
                    // alert(pro[1] + "_" + pro[0]);
                    select_ddlWZLB1.options.add(new Option(pro[1], pro[0]));
                }

                Request_2Ji();
            }
        }

        function Request_2Ji() {
            select_ddlWZLB1 = document.getElementById("ddlWZLB1"); 
            select_ddlWZLB2 = document.getElementById("ddlWZLB2");
            select_ddlWZLB2.options.length = 0;
            xmlhttp.onreadystatechange = Load_2Ji;
            xmlhttp.open("GET", "../OutRegister/DealRequestServices.ashx?requesttypeid=2&aStr=" + select_ddlWZLB1.value, true);
            xmlhttp.send(null);
        }

        function Load_2Ji() {
            select_ddlWZLB2 = document.getElementById("ddlWZLB2"); 
            if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
                var cities = xmlhttp.responseText.split("|");
                for (var i = 0; i < cities.length; i++) {
                    var city = cities[i].split(";");
                    select_ddlWZLB2.options.add(new Option(city[1], city[0]));
                }
                // RequestCounty();
            }
        }

        function ShowSelectValue() {
            select_ddlWZLB1 = document.getElementById("ddlWZLB1");
            select_ddlWZLB2 = document.getElementById("ddlWZLB2");
            document.getElementById("hidLB1").value = select_ddlWZLB1.options[select_ddlWZLB1.selectedIndex].text;
            document.getElementById("hidLB2").value = select_ddlWZLB2.options[select_ddlWZLB2.selectedIndex].text;
            document.getElementById("hidLB_flh").value = select_ddlWZLB2.options[select_ddlWZLB2.selectedIndex].value;
            show.innerHTML = "已选择：" + select_ddlWZLB1.options[select_ddlWZLB1.selectedIndex].text + "<font color=red>" + select_ddlWZLB2.options[select_ddlWZLB2.selectedIndex].text + "</font>"; //  + select_ddlWZLB2.options[select_ddlWZLB2.selectedIndex].value;
            // alert(document.getElementById("hidLB1").value + "_" + document.getElementById("hidLB2").value);
        }

        /////////////////
        function InstantiateXMLHttp2() {
            //Mozilla 浏览器
            if (window.XMLHttpRequest) {
                xmlhttp2 = new XMLHttpRequest();
                //设置MiME类别
                if (xmlhttp2.overrideMimeType) {
                    xmlhttp2.overrideMimeType('text/xml');
                }
            }
            //IE浏览器
            else if (window.ActiveXObject) {
                //IE 7、8
                try {
                    xmlhttp2 = new ActiveXObject("Msxml2.XMLHTTP");
                }
                //IE 6
                catch (e) {
                    xmlhttp2 = new ActiveXObject("Microsoft.XMLHTTP");
                }
            }

            Request_zy();
        }
        
        function Request_zy() {
            select_ddl_ZY = document.getElementById("ddl_ZY");
            select_ddl_ZY.options.length = 0; //请空下拉列表框
            xmlhttp2.onreadystatechange = Load_zy; //设置当XMLHttpRequest对象的状态改变时调用的方法
            xmlhttp2.open("GET", "../OutRegister/DealRequestServices.ashx?requesttypeid=3", true); //设置XMLHttpRequest对象请求的方式、地址和是否异步
            xmlhttp2.send(null); //发送请求
        }
        function Load_zy() {
            select_ddl_ZY = document.getElementById("ddl_ZY");
            if (xmlhttp2.readyState == 4 && xmlhttp2.status == 200) {
                var provinces = xmlhttp2.responseText.split("|");
                for (var i = 0; i < provinces.length; i++) {
                    var pro = provinces[i].split(";");
                    // alert(pro[1] + "_" + pro[0]);
                    select_ddl_ZY.options.add(new Option(pro[1], pro[0]));
                }

               // Request_2Ji();
            }
        }
        function ShowSelectValue_zy() {
            select_ddl_ZY = document.getElementById("ddl_ZY");
            document.getElementById("hidZY").value = select_ddl_ZY.options[select_ddl_ZY.selectedIndex].text;
        }

        /// 服务
        /////////////////
        function InstantiateXMLHttp3() {
            //Mozilla 浏览器
            if (window.XMLHttpRequest) {
                xmlhttp = new XMLHttpRequest();
                //设置MiME类别
                if (xmlhttp.overrideMimeType) {
                    xmlhttp.overrideMimeType('text/xml');
                }
            }
            //IE浏览器
            else if (window.ActiveXObject) {
                //IE 7、8
                try {
                    xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
                }
                //IE 6
                catch (e) {
                    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
                }
            }

            Request_1Ji_fw();
        }

        function Request_1Ji_fw() {
