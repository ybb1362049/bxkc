    function globSearch(){
       var sUrl = "http://dig.lsz.gov.cn/dig/search.action?ty=&w=false&f=&dr=true&p=1&sr=score+desc&fq=&advtime=&advrange=&q=";
        var key = document.getElementById("search1016").value;
        if(key == "" || key == "请输入关键字"){
          alert("关键字不能为空!");
          return false;
        }

        key = encodeURI(key);
        sUrl = sUrl + key + '&ext=siteid:1626702'
        window.open(sUrl);
    }

$(function(){$(".nav_bg ul li:last").css("background","none");})

        function enterInput(evt) {
            var evt = evt ? evt : (window.event ? window.event : null); //兼容IE和FF
            if (evt.keyCode == 13) {
                document.getElementById("aSearch").click();
            }
        }

</script>
