<script>
//比较两个时间的大小
function cha(timeStr1, timeStr2){
	var time1 = new Date(Date.parse(timeStr1));
	var time2 = new Date(Date.parse(timeStr2));
	return time1.getTime() - time2.getTime();
}
$(document).ready(function(){
	var currentTime = $("#currentTime1").val();
	currentTime = new Date(Date.parse(currentTime.replace(/-/g,"/")));
	
	$("#bulletinListDetail").find(".photo").each(function(i, domEle){
		var tenderMethod = '06';
		var bulletinType = $(domEle).find("input[name=bulletinType]").val(); //公告类型
		var projImplStatus = $(domEle).find("input[name=projImplStatus]").val();
		var projProcessStatus = $(domEle).find("input[name=projProcessStatus]").val();
		var startTime = ''; //开始时间
		var endTime = ''; //结束时间
		
		//招标公告
		if(bulletinType == '12'){
			startTime = $(domEle).find("input[name=evalStartTime]").val();
			endTime = $(domEle).find("input[name=evalEndTime]").val();
			
			startTime = new Date(Date.parse(startTime.replace(/-/g,"/")));
			endTime = new Date(Date.parse(endTime.replace(/-/g,"/")));
		}
		var imgName = 'notes.png';
		if(bulletinType == '13'){
			var ebuyMethod = $('#ebuyMethod').val();
			if(ebuyMethod == '06'){
				imgName = 'notes_jj.png'
			}
			if(ebuyMethod == '21'){
				imgName = 'notes_zg.png'
			}
		}

		//竞价
		if(bulletinType == '12' && tenderMethod == '06') {
			
			//未开始  开始时间 > 当前时间  并且不等于终止
			//进行中  开始时间 < 当前时间 < 结束时间  并且不等于终止 
			//已完成  当前时间 > 结束时间   或者已终止

			if(startTime > currentTime && projImplStatus != '02') {
				imgName='nostart.png';
			} else if(startTime < currentTime && currentTime < endTime && projImplStatus != '02' && projProcessStatus != '200') {
				imgName='being.png';
			} else if(currentTime > endTime || projImplStatus == '02' || projProcessStatus == '200') {
				imgName='over.png';
			}
			
		}

		//直购
		if(bulletinType == '12' && tenderMethod == '21') {
			if(projImplStatus == '03') {
				imgName='being.png';
			} else if(projImplStatus == '04' || projImplStatus == '02') {
				imgName='over.png';
			}
		}
		
		$(domEle).find("span").html('<img src="view/resource/skin/pubservice/img/'+imgName+';jsessionid=yDyHhcQp18V4Bn7KhwMQJ2bpK6C96X5hK5WDdYWPj6HyYKJ7pG51!-1790893622">');
	});
});
</script>
