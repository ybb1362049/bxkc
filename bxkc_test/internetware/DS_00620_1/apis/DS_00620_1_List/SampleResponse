<script type="text/javascript">
$(document).ready(function(){
	var pageType=12;
	var tenderMethod1=06;
	if(pageType=='13'){
		$("#menu li").removeClass();
		$("#dealBulletin1").addClass("selected");
	}
	if(pageType=='12'){
		if(tenderMethod1=='6'){
			$("#menu li").removeClass();
			$("#purBulletin1").addClass("selected");
		}
		if(tenderMethod1=='21'){
			$("#menu li").removeClass();
			$("#purBulletin2").addClass("selected");
		}
		if(tenderMethod1=='3'){
			$("#menu li").removeClass();
			$("#correctBulletin1").addClass("selected");
		}
	}
	if(pageType=='15'){
		$("#menu li").removeClass();
		$("#correctBulletin1").addClass("selected");
	}
})
