		onmouseover = _("c=1");
		onmouseout = _("c=0"); 
	}
	(F = _("if(#%45||!c)#++,#%=o.scrollHeight>>1;setTimeout(F,#%45?10:1500);"
			.replace(/#/g, "o.scrollTop")))();
   </script>
</body>
</html>