function checksearch7987(type){
        var keyword = document.getElementById('showkeycode7987').value;
        if(keyword==null||keyword==""){
            alert("请输入你要检索的内容！");
            return false;
        }
        if(window.toFF==1)
        {
            document.getElementById("lucenenewssearchkeyword7987").value = Simplized(keyword );
        }else
        {
        	document.getElementById("lucenenewssearchkeyword7987").value = keyword;            
        }
        document.getElementById('_lucenesearchtype7987').value=1;
        var  base64 = new Base64();
        document.getElementById("lucenenewssearchkeyword7987").value = base64.encode(document.getElementById("lucenenewssearchkeyword7987").value);
        new VsbFormFunc().disableAutoEnable(document.getElementById("showkeycode7987"));
        return true;
    } 

