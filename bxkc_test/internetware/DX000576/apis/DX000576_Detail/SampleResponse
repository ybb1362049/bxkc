function convert(compId,item){
		var detailform = document.getElementById(compId + "_" + "form");
	    var newsKeyword_show = detailform.newsKeyword.value;
		if(newsKeyword_show != ""){
			var keyV = document.getElementById(item).innerText;
			if(!keyV){
				keyV = document.getElementById(item).innerHTML;
			}
			var after = encodeURIComponent(keyV);
			detailform.newsKeyword.value=after;
		}
		detailform.action="/FrontNews.do?method=submit";
		detailform.submit();
}
function convertStr(item){
	return item.replace(/\&lt;/g, "<").replace(/\&gt;/g, ">"); 
}
function gotohtml(){
	if(window.i18n_FrontNews_detail01_news_page_haveNoRight){
			alert(i18n_FrontNews_detail01_news_page_haveNoRight);
		}
		var pageNO = history.length;
		if (document.all) {
            if (pageNO == 0) {
                window.close();
            }
            else {
                history.go(-1);
            }
        }
        else {
            if (pageNO == 1) {
                window.close();
            }
            else {
                history.go(-1);
            }
        }
}
</script>
