	function fileDownLoad(file_id,file_type_id) {
		if (file_type_id =='招标文件' || file_type_id =='资格预审文件'){
			window.location = "http://218.2.208.144:8094/EBTS/placard/announcebaseinfo/doFileDownload1?file_id=" + file_id;
		}else{
			//下载
	 		window.location = "http://218.2.208.144:8094/EBTS/placard/announcebaseinfo/doFileDownload?file_id=" + file_id;
		}
		
	}
	
	
	function setInitialise(){
		if('0'!=null &&'0'!='' )
		{
			var obj=document.getElementsByName('documentOnlineCheck');
			for(var i=0; i<obj.length; i++){ 
				//等于0的时候 支持线上支付与线下支付
				if('0'==0)
					{
					obj[i].checked=true;
					}
				else
					{
					//1和2的情况下要值相同才能打勾
					if(obj[i].value=='0') 
					{
						obj[i].checked=true;
					}
					}
				
			}
		}
		
	}
	
	$(document).ready(function(){
		//初始化checkbox按钮
		setInitialise();
	});
	</script>
