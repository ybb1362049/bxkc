<script type="text/javascript">
function lockInfo(){
	var tender_id=$("#tender_id").val();
	var zb_placard_id=$("#zb_placard_id").val();
	var zb_unit_id=$("#zb_unit_id").val();
	$('#grid').datagrid({
		url:'http://218.2.208.144:8094/EBTS/publish/announcement/ajaxDoLockInfo?tender_id='+tender_id+'&zb_placard_id='+zb_placard_id+'&zb_unit_id='+zb_unit_id, 
		idField:'user_id',
		fitColumns:true ,  
		singleSelect:true,
		columns:[[   
                   {field:'lock_flag',title:'锁定/非锁定',width:150,
                	   formatter:function(value,row,index){
                		   if ("0" == value){
                			   return "<input type='checkbox' disabled='disabled'  name='"+row.user_id+"' value='"+value+"' >";
                		   }else if ("1" == value) {
                			   return "<input type='checkbox' disabled='disabled' name='"+row.user_id+"' checked='true' value='"+value+"' >";
                		   }else{
                			   return "<input type='checkbox' disabled='disabled' name='"+row.user_id+"' value='"+value+"' >";
                		   }
                   }},
                   {field:'tb_employee_name',title:'人员名称',width:150},   
                   {field:'this_project_position',title:'人员职位',width:200}
              	]]
		});		
}

$(function(){
 	lockInfo();
});
</script>
