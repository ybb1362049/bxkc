  <script>
function searchBy(){

	   searchPage = 1;//默认第一页
	    var queryString = "";
 		var pageSize = 10;

		var dockeywords    = $("#dockeywords").val(); 
		if(dockeywords==null||dockeywords==""){
			window.open("/cptqt/web/search/index.jsp");
		}else{		
		queryString += "&dockeywords="+dockeywords;	
	 	var searchData = queryString;//保存搜索条件
	 	var formData=searchData.replace(/&/g,"}");//替换特殊字符，在地址栏传输
		formData = decodeURIComponent(formData, true);
	 	window.open("/cptqt/web/search/resuilt.jsp?formData="+formData+"}dockeyonewords=}keytitle=1}&pageSize="+pageSize,'_blank');
	}
}
</script>
