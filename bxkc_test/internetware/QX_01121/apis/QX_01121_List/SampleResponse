	function goSearch()
	{
		var searchUrl="http://wza.cncx.gov.cn/yxsm/?m=search&a=query&si=35065&key=";
		var words=document.getElementById("keyword").value;
		var formNode=document.getElementById("cniil_searchform");
		var actionUrl=words?searchUrl+encodeURIComponent(words):homeUrl;
		formNode.action=actionUrl;
		formNode.submit();
	}
	function enter(evt)
	{
		evt = evt ||window.event; 
		var key=evt.which||evt.keyCode;
		if(key==13)goSearch();
