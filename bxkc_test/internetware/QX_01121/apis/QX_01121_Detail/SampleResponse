     <script type="text/javascript">
          /*搜索查询*/
          function SiteSearch(send_url) {
              var kw = $.trim($(keyword).val());
              //if (kw.length > 0 && kw != "") {
              window.location.href = send_url + "?keyword=" + encodeURI(kw);
              //}
          }
 $(function () {
              $("input[name='keyword']").keydown(function (e) {
              var ev = document.all ? window.event : e;
              if (ev.keyCode == 13) {
                  var val = $("input[name='keyword']").val();
                  if ($.trim(val) != "") {
                      location.href = "search.html?keyword=" + encodeURI(val);
                      return false;
                  }
                  else {
                      return false;
                  }
              }
          });
          });
      </script>
