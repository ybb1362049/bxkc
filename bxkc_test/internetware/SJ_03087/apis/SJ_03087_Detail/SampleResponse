	<script>
	function  oncous()
          {
            document.getElementById("searchkey").value="";
          }
            function  search()
            {
            var values=escape(document.getElementById("searchkey").value);
            var url="search.html?type=title&tags="+values;
           if(values=="")
           {
           alert("请输入关键字")
           }
           else{
            window.location=url;
         }
            }
            </script>
		
		
