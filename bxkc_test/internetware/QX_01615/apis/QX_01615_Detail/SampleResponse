	function  SearchSub(key)
		{
			//必需，先将文本转换为utf-8
	        document.charset = "utf-8";
	        var keyword= key;
	        keyword = keyword.replace(/\~|\!|\*|\?|\^|\\|\(|\)|\[|\]|\+|\-|\:|\{|\}|\"/g, "");
		    window.open('/search.html?keyword=' + encodeURI(keyword)); //传入参数
		}
