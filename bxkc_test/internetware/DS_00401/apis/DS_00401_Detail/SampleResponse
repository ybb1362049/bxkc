        function tab() { if (event.keyCode == 13) { document.getElementById("submit").click(); } }
        function search() {
            var tim = 10
            if (tim == 10) {
                tim = 0;
                if (document.getElementById("txt").value == "" || document.getElementById("txt").value == "关键字") {
                    //alert("请输入关键字！");
                }
                else {
                    var t = document.getElementById("txt").value;
                    if (t.indexOf("<") > -1 || t.indexOf(">") > -1) {
                        alert("请勿在关键字中输入非法字符!");
                        return;
                    }
                    var openUrl = '/ggzy/ShowInfo/SearchResult.aspx?keyword=';

                    openUrl += escape(t) + "&searchtype=title";
                    window.open(openUrl);
                }
            }
            else {
                alert("两次搜索的时间间隔不能少于十秒")
            }
        }

</script>

