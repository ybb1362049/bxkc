}

function selsss()
{
	var index_sel= document.getElementById("index_sel");
		if(index_sel.value=="")
		{
			alert("搜索内容不能为空！");
			return;
		}
		window.location.href="search_product_s_2.asp?keywords="+index_sel.value;
}

