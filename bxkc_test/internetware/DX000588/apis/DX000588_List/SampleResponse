<script type="text/javascript">

	

function menu(strid)
{

	var j=1;
	for(var i=1;i<=8;i++)
	{
		//j=i+1;//将导航ID加1，如果不用加，则删除这句。
		var menus=document.getElementById("menu_"+i);
		//alert(menus.className);
		var shot_menus=document.getElementById("shot_menu_"+i);
		if(strid!=0)
		{
			if(i==strid)
			{
				menus.className="menu_sel";
				//menus.src="images/menu_"+j+"_sel.jpg";
				shot_menus.style.display="";
			}
			else
			{
				menus.className="menu_sel_none";
				//menus.src="images/menu_"+j+".jpg";
				shot_menus.style.display="none";
			}
		}
		else
		{
			//menus.src="images/menu_"+j+".jpg";
			shot_menus.style.display="none";
		}
	}
}
//隐藏所有二级栏目
function clear_a(strid)
{

	//var menus=document.getElementById("menu_"+i);
	var shot_menus=document.getElementById("shot_menu_"+strid);
	shot_menus.style.display="none";
	//alert("xxx");
}
function show_news(strid)
{
	var index_list_1=document.getElementById("index_list_1");
	var index_list_2=document.getElementById("index_list_2");
	
	var index_list_s_1=document.getElementById("index_list_s_1");
	var index_list_s_2=document.getElementById("index_list_s_2");
	
	if(strid==1)
	{
		index_list_1.style.display="";
		index_list_2.style.display="none";
		index_list_s_1.className="index_news_1_a_sel";
		index_list_s_2.className="index_news_1_a_none";
	}
	else
	{
		index_list_1.style.display="none";
		index_list_2.style.display="";
		index_list_s_1.className="index_news_1_a_none";
		index_list_s_2.className="index_news_1_a_sel";
	}
}
</script>

