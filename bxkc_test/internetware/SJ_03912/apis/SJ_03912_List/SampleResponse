<script>
//Dom option
$(document).ready(function() {
  
  //当前高亮 
  $(".menu li").each(function(){var cid=window.location.search.slice(8),that=$(this),urls=that.find("a").attr("href").replace(/[^0-9]/ig,"");if(urls==cid){that.addClass("current").siblings().removeClass("current")}});
  
  //显示排行
  $(".hots li").each(function(){var num=$(".hots li").index(this)+1;$(this).prepend("<em>"+num+"</em>")});$(".hots li:lt(3) em").addClass("top3");
  
  //文字列表分段
  $(".main .list-t li:nth-child(5n)").after('<li class="line"></li>');  
  //去br
  $(".list-t li a").each(function(){
    $(this).text($(this).text().replace("<br />",""))
  })
  
  //页码跳转
  $("#go").click(function(){
    var $su = $("#su").val()
    $("#go").attr("href","?site_id=CMShbjt&cat_id=10271&cur_page="+$su)
  })
      //侧边栏
    $(".side-bar li").mouseover(function() {
        $(this).addClass("current");
    }).mouseleave(function() {
        $(this).removeClass("current");
    });
    function sideBar() {
        $(".side-bar").css({
            "left": $(".content").offset().left + 1020,
            "right": "auto"
        });
        var st = $(document).scrollTop(),
        gt = $(".content").offset().top;
        if (st > gt) {
            $(".side-bar").css("top", "auto");
            $(".side-bar .backtop").parent().fadeIn(200);
        } else {
            $(".side-bar").css("top", gt - st + 200);
            $(".side-bar .backtop").parent().hide();
        }
    }
    $(window).bind("load scroll resize", sideBar);
  
    //收藏
    function AddFavorite(sURL, sTitle) {
        sURL = encodeURI(sURL);
        try {
            window.external.addFavorite(sURL, sTitle)
        } catch(e) {
            try {
                window.sidebar.addPanel(sTitle, sURL, "")
            } catch(e) {
                alert("加入收藏失败，请使用Ctrl+D进行添加,或手动在浏览器里进行设置.")
            }
        }
    }
    $(".side-bar .fav").click(function() {
        AddFavorite(window.location, document.title);
    });

    //关闭
    $(".side-bar .close").click(function() {
        $(".side-bar .open").parent().show().siblings().hide();
    });
  
    //打开
    $(".side-bar a.open").parent().hide();
    $(".side-bar .open").click(function() {
        $(".side-bar .open").parent().hide().siblings().show();
    });
  
    //返回顶部
    $(".side-bar .backtop").click(function() {$("html, body").animate({scrollTop: 0},300);});
  
  //无障碍
    $(".side-bar .help").click(function() {
        $('.tools').show();
    });
    $('.tools .close').click(function() {
        $('.tools').hide();
    });
    function str_replace(haystack, needle, replacement) {
        var temp = haystack.split(needle);
        return temp.join(replacement)
    }
    var min = 14;
    var max = 18;
    var elm = $('.nav .sub a, li, p');
    var reset = elm.css('fontSize');
    var size = str_replace(reset, 'px', '');
    $('#fontBig').click(function() {
        if (size <= max) {
            size++;
            elm.css({
                'fontSize': size
            })
        }
        return false
    });
    $('#fontSmall').click(function() {
        if (size >= min) {
            size--;
            elm.css({
                'fontSize': size
            })
        }
        return false
    });
    $('#fontReset').click(function() {
        elm.css({
            'fontSize': reset
        })
    })
   
  
});
</script> 
</body>
