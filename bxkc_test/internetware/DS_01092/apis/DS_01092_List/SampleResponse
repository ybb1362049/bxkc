<script>
var pageDirection = {};
//跳转
pageDirection.jump = function(page){
	$("#page").val(page);
	show_list.makeSearchData();
}

$(document).ready(function(){
	//第一页点击
	$("#QuotaList_first").click(function(){
		pageDirection.jump('1');
	});
	
	//上一页点击
	$("#QuotaList_previous").click(function(){
		pageDirection.jump(parseInt($("#page").val())-1);
	});
	
	//下一页点击
	$("#QuotaList_next").click(function(){
		pageDirection.jump(parseInt($("#page").val())+1);
	});
	
	//最后页点击
	$("#QuotaList_last").click(function(){
		pageDirection.jump($("#totalPageNum").val());
	});
	$("[name=myShow]").each(function(){
       if(""!=$(this).attr("scores")){
    	   var imgsrc = getShows($(this).attr("scores"));
    	   $(this).append(imgsrc);
       }
	});
});
//级别开始
function getShows(score){
	
    var alls = "";
	var imgsrczhuan= '<img src="view/resource/images/zhuan.gif"/>';
	var imgsrchuanguan= '<img src="view/resource/images/jinguan.gif"/>';
	var scorestring = score+"";
	if(null==scorestring||''==scorestring||'undefined'==scorestring){
		scorestring='0';
	}
	var scores = scorestring.split(".");
	if(Number(scores[0])>=5500){//10级
		for(var k=1;k<=10;k++){
			alls+=imgsrchuanguan;
		}
	}else if(Number(scores[0])>=4500){//9级
		for(var k=1;k<=9;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-4500)/100)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])>=3600){//8级
		for(var k=1;k<=8;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-3600)/90)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])>=2800){//7级
		for(var k=1;k<=7;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-2800)/80)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])>=2100){//6级
		for(var k=1;k<=6;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-2100)/70)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])>=1500){//5级
		for(var k=1;k<=5;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-1500)/60)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])>=1000){//4级
		for(var k=1;k<=4;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-1000)/50)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])>=600){//3级
		for(var k=1;k<=3;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-600)/40)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])>=300){//2级
		for(var k=1;k<=2;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-300)/30)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])>=100){//1级
		for(var k=1;k<=1;k++){
			alls+=imgsrchuanguan;
		}
		var num = Math.floor((Number(scores[0])-100)/20)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}else if(Number(scores[0])<100){//0级
		var num = Math.floor((Number(scores[0]))/10)
		for(var k=1;k<=num;k++){
			alls+=imgsrczhuan;
		}
	}
    return alls;
}
</script>
</div>
					
					
					
				</div>
				<!--产品信息 结束-->
			</div>
		</div>
	</div>
	<!--主要内容 结束-->
