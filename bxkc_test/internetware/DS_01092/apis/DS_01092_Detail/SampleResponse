<script type="text/javascript">
$(document).ready(function(){
	var pageType=12;
	var tenderMethod1=06;
	if(pageType=='13'){
		$("#menu li").removeClass();
		$("#dealBulletin1").addClass("selected");
	}
	if(pageType=='12'){
		if(tenderMethod1=='06'){
			$("#menu li").removeClass();
			$("#purBulletin1").addClass("selected");
		}
		if(tenderMethod1=='21'){
			$("#menu li").removeClass();
			$("#purBulletin2").addClass("selected");
		}
		if(tenderMethod1=='03'){
			$("#menu li").removeClass();
			$("#correctBulletin1").addClass("selected");
		}
	}
	
	var goodsTd = $("#goodsTd");
	if(goodsTd != null && goodsTd != 'undefined') {
		var brands = $("#goodsTd").html(); 
		var resultStr = brands.replace(/(^\s*)|(\s*$)/g, "");
		//var resultStr = brands.replace(/\ +/g, ""); //去掉空格
        //resultStr = resultStr.replace(/[ ]/g, "");    //去掉空格
        resultStr = resultStr.replace(/[\r\n]/g, ""); //去掉回车换行

		if(resultStr == '') {
			$("#goodsTd").html("不限制");
		}
	}
	
})
</script>
