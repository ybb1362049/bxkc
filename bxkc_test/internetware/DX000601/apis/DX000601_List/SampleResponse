<script type="text/javascript" src="https://jcs-caigou.mingyuanyun.com/res1.5/plugins/datepicker/WdatePicker.js?c_version=2018-01-12"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $("#RegisterEndDateStart").focus(function () {
            WdatePicker({ readOnly: true, maxDate: '#F{$dp.$D(\'RegisterEndDateEnd\')}' });
        });
        $("#RegisterEndDateEnd").focus(function () {
            WdatePicker({ readOnly: true, minDate: '#F{$dp.$D(\'RegisterEndDateStart\')}' });
        });

        // 分公司-项目联动
        $("#search_area").on('change', '#SubCompanyId', function (e, callback) {
            var _e = $(this),
                $selcLevel = $("#search_area").find('#ProjectId');
            var buguid=$("#SubCompanyId").val();
            $.post("/xykg/web_developerzone/home/get-project", { buguid: buguid }, function (data) {
                var htmlStr = "<option value=\"\">请选择</option>";
                if (_e.val() == "") {
                    htmlStr = "<option value=\"\">所有项目</option>";
                }
                if (data) {
                    for (var i = 0; i < data.length; i++) {
                        htmlStr += "<option value=\"" + data[i].id + "\">" + data[i].name + "</option>"
                    }
                }
                $selcLevel.html(htmlStr);
                if (typeof callback === 'function') {
                    callback.call($selcLevel[0]);
                }
            });
        });
    });
</script>

