	<script type="text/javascript">
		$(function(){
			var iChild=$('#children a').length;
			if(iChild<1){
				$("#children").hide();
			}
		  function fns(agr){
		var codeName="<CMSPRO_CHANNEL CODE='' FIELD='channelName'></CMSPRO_CHANNEL>";
		  $(agr).each(function(e){
			if($(this).text()==codeName){
				$(this).addClass("zwfwl1");
			 }
		  });
		 }
		  fns('#lanM ul li');
		  
		  var nums=$('.listBoxs li').length;
		  for(var i=0;i<=nums;i++){
		  	if(((i+1)%5) == 0){
		  		$('.listBoxs li').eq(i).addClass('uli');
		  		$('.listBoxs li').eq(i+1).addClass('uli1');
		  	}
		  }
		  
		  var  sText = $('#current').text();
		  var  aText = sText.split('>');
		  var  sStr=$.trim(aText[1]);
		  $('#menu ul li').each(function(e){
		  	if($(this).text()==sStr){
				$(this).addClass("active");
				var muls = $("#menu ul li").size();
				for (var i = 0; i < muls; i++) {
					$("#menu ul li a").eq(i).css("background","url(/wuhou/xhtml/images/icon"+(i+1)*11+".png) 14px 15px no-repeat");
					if(i == (muls - 1)){ 
						$("#menu ul li a").eq(i).css("background","url(/wuhou/xhtml/images/icon"+(i+1)*11+".png) 12px 15px no-repeat");
					}
					
				};
			 }
		  });
		});
	</script>
