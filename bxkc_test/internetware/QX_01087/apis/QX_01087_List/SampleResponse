              function page_go(){
                var locationUrl = "/es-search/search/5b65f37586a940db95bec117651f9b1a?_isAgg=0&amp;_pageSize=20&amp;_template=wuhou_list&amp;_channelName=%E6%8B%9B%E6%A0%87%E5%85%AC%E5%91%8A&amp;_allNum=0&amp;page=";
