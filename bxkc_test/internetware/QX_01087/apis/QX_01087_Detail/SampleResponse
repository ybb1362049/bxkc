	<script type="text/javascript">
		$(function(){
			//搜索框
			var seach = $(".searchText");
			var oldVal = seach.val();
			seach.focus(function(){   //获取光标
				if($(this).val()==oldVal){
					$(this).val('');
				}
				$(this).css("color","#000");
			});
			seach.blur(function(){   //失去光标
				if($(this).val()==""){
				   $(this).val(oldVal);
				   $(this).css("color","#b8b8b8");
				}
			})

		});
	</script> 
