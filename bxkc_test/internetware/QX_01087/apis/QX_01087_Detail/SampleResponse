					console.log(msg);
					var data = msg.data.result;
					for(var i=0;i<data.length;i++){
						var title = data[i].TITLE;
						var url = data[i].URL;
						
                      $("#rmwz >ul.ul").append("<li><a href='http://www.cdwh.gov.cn/wuhou/"+url+"' target='_blank'>"+title+"</a></li>");
					}
					 }
                    });
				

    })
    
  </script>
  
  
</body>
</html>