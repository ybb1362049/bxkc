    function ShowMessage(txt, msgtype) {
        var imgurl = "images/";
        switch (msgtype) {
            case "success":
                imgurl = imgurl + "dialog_right.png";
                break;
            case "fib":
                imgurl = imgurl + "dialog_forbidden.png";
                break;
            default:
                imgurl = imgurl + "dialog_forbidden.png";
                break;

        }

        new Alert('but', 'box', {
            title: '提示信息',
            content: '<div class="size"><div class="info_area"><img style="width:32px;height:32px;" align="absmiddle"  src=' + imgurl + '  /><span class="text_area">' + txt + '</span></div></div><div class="but"><button class="button">确定</button></div>',
            width: '',
            height: '180',
            top: '300px',
            left: '',
            fixed: '',
            close: 'close'
        });

        document.getElementById('but').click();


    }


    function ShowVisitMethod() {


        new Alert('but', 'box', {
            title: '提示信息',
            content: '<div class="size"><div class="info_area"><img style="width:32px;height:32px;" align="absmiddle"  src=' + imgurl + '  /><span class="text_area">' + txt + '</span></div></div><div class="but"><button class="button">确定</button></div>',
            width: '',
            height: '180',
            top: '300px',
            left: '',
            fixed: '',
            close: 'close'
        });

        document.getElementById('but').click();



    }


    function RedirectUrl(url) {
        window.location.href = url;

    }

    function VipSignUpLink(type, links) {


        $('#divAd').hide();
        var infohtml = "";

        var height = "200px";
        if (type == "1") {
            infohtml = $('#feivip').html();
        } else {
            infohtml = $('#isvip').html();
            infohtml = infohtml.replace('#rlink', links);
        }


        new Alert('but', 'box', {
            title: '报名通道',
            content: '<div class="size">' + infohtml + '</div>',
            width: '300px',
            height: height,
            top: '300px',
            left: '',
            fixed: '',
            close: 'close'
        });

        document.getElementById('but').click();



    }


    function SignUpInfoHide() {
        document.getElementById('L_Close').click();
    }


  
