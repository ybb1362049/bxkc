     function OperateEWM(type) {

         switch (type) {
             case 0: //隐藏二维码

                 $("#div_control").animate({
                     right: '0px'
                 });

                 $("#div_ewm").animate({
                     right: '-456px'
                 });

                 $('#con_hide').hide();
                 $('#con_show').show();
                 break;

             case 1: //显示二维码
                 $("#div_control").animate({
                     right: '455px'
                 });

                 $("#div_ewm").animate({
                     right: '0px'
                 });

                 $('#con_hide').show();
                 $('#con_show').hide();

                 break;

         }

     }




