    <li class="gt" id="gt"><a href="javascript:;" title="返回顶部"></a></li>
</ul>
<script>
    $(".sideFixed li").hover(function () { $(this).addClass("on") }, function () { $(this).removeClass("on") });
     $(function(){
     $("#gt").click(function(){
     $("html,body").animate({scrollTop:0},1000);
     return false;
     });
      });
</script>

