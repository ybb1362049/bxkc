<script type="text/javascript">
    function search() {
        if (document.all.keyword.value == ''||document.all.keyword.value == '站内搜索') {
            alert('搜索字段不能为空！');
        } else {
            var openUrl = '../showinfo/searchresult.aspx?CategoryNum=&keyword=';
            openUrl += escape(document.all.keyword.value);
            openUrl += '&searchtype=' + escape(document.getElementById('selectop').value);

            window.open(openUrl, 'search', '', '');
        }
    }
	function CheckSearch()
		{
			if(event.keyCode=="13")
			{   
				if(document.all.txtKey.value!="")
				{
					search();
				}
				else
				{
					alert("搜索字段不能为空！");
				}
			}
		}
		function Clear() {
             document.all.txtKeys.value = ""
}

</script>
