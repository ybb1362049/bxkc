		<script>
			$(".panel-search li").on("click", function(e) {
				e.stopPropagation();
				if($(this).hasClass("disabled")){
					return false;
				}
				$(this).siblings().removeClass("on");
				$(this).addClass("on");
				$(this).parent().siblings().removeClass("on");
				
			})
			$(".panel-tab >li").on("click", function(e) {
				e.stopPropagation();
				if($(this).hasClass("disabled")){
					return false;
				}
				var url=$(this).attr("data-href");
				window.location.href=url;
			})
			
		</script>
