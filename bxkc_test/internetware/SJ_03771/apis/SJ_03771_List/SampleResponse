	<script>
	$(function () {
        if ($('.hotWordBanner').size() > 0) {
            var hotWordBanner = (function (externalO) {
                var externalGo = true, _index = 0, externalO = '.' + externalO;
                $(externalO).hide().children().hide();
                function inside() {
                    $(externalO).eq(_index).children().first().fadeIn(function play() {
                        $(this).next().fadeIn(play);
                        if ($(this).index() == $(externalO).eq(_index).children().size() - 1) {
                            _index++;
                            externalGo = true;
                            setTimeout(playGo, 3000);
                        }
                        return 'slow';
                    })
                }
                function external() {
                    $(externalO).eq(_index).fadeIn('fast').siblings(externalO).hide(function () {
                        externalGo = false;
                        inside();
                        return 0;
                    }).children().hide();

                }
                function playGo() {
                    if (externalGo == true) {
                        _index >= $(externalO).size() ? _index = 0 : "";
                        external();
                    }
                }
                return playGo();
            })('hotWordBanner');
        }
    }
)
	</script>
