				function fun1() {
					alert("您即将离开北京市公共资源交易服务平台网站");
				}
				$(".btn-search").on("click", function() {
					var url ="https://search.bjggzyfw.gov.cn/elasticsearch/index.jsp";
					document.getElementById("search-form").action = url;
					$(".searchAll").val($(".searchAll").val().trim().replace(/\s/g,""));
					$("#search-form").submit();
				})
				$(".searchAll").on("keydown", function(event) {
					if (event.keyCode == 13) {
						$(".btn-search").click();
						return false;
					}
				})
			</script>
