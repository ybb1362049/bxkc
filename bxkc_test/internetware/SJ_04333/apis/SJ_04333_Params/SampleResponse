<script language="javascript">

function loadmenu()
{
    var typeID= parseInt(document.getElementById("ctl00_cphBannerAndMain_hdfTypeID").value);
    var dl = "dl" + typeID;
    if( typeID != 31 &&typeID<39)
    {
        document.getElementById(dl).className = "current_2";
        if( typeID == 36 || typeID == 37 || typeID == 38 )
            document.getElementById("dlMenu").style.display = "block";
        
    }
    else if( typeID == 31)
    {
        document.getElementById("dl31").className = "current_2"; 
    }
}

loadmenu();

</script>
            	        
            
                
              
