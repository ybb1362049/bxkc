<script type="text/javascript">
	function deleteTextvalue(){
		document.getElementById("keyword").value = "";
	}
	
	// 键盘enter事件
	function checkEnter(){
		// 查询内容
		var keyword = document.getElementById("keyword").value;
		keyword = all_trim(keyword);
		document.getElementById("searchfmOfAtic").action = "system/common/searchResult.jsp?keyword="+encodeURIComponent(keyword);
	}

	function click(){
		document.all.formsubmit.submit();
	}
</script>
