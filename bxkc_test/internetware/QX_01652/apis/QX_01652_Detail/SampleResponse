<script type="text/javascript">
	jQuery(document).ready(function(){
		jQuery(".ynav ul li").removeClass("on");
		var str=location.pathname.substring(1,5);
		//alert(str);
		if(str=="xwzx"){
			jQuery(".ynav li:eq(1)").addClass("on");
		}else if(str=="zwgk"){
			 jQuery(".ynav li:eq(2)").addClass("on");
		}else if(str=="zxfw"){
			 jQuery(".ynav li:eq(3)").addClass("on");
		}else if(str=="hdjl"){
			 jQuery(".ynav li:eq(4)").addClass("on");
		}else if(str=="zjal"){
			jQuery(".ynav li:eq(5)").addClass("on");
		}else if(str=="tzal"){
			jQuery(".ynav li:eq(6)").addClass("on");
		}else if(str=="yxal"){
			jQuery(".ynav li:eq(7)").addClass("on");
		}else if(str=="snxx"){
			jQuery(".ynav li:eq(8)").addClass("on");
		}else if(str=="ztzl"){
			jQuery(".ynav li:eq(9)").addClass("on");
		}else{
			jQuery(".ynav li:eq(0)").addClass("on");
		}
	});
</script> 
