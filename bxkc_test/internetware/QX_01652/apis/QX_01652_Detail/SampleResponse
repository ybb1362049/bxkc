							<script type="text/javascript">
								var html="";
								//alert("111");
								var shiping="";
								var xgtp="";
								var splj="";
								//alert(shiping);
								if(shiping!=""&&shiping!=null)
								{
									html+="<div class=\"video\">";
									html+="<div class=\"controls\" style=\"width: 600px;height:400px;margin:0 auto;\">";
									html+="<iframe id=\"videoFrame\" src=\""+shiping+"\" frameborder=\"0\" style=\"width:600px;height:400px\" scrolling=\"auto\"></iframe>";
									html+="</div>";
									html+="</div>";
									jQuery("#sp").append(html);
								}else if(splj!=""&&splj!=null){
									var strs= new Array(); //定义一数组 
									strs=splj.split("."); //字符分割 
									//alert(strs[strs.length-1]);
									if(strs[strs.length-1]=="flv"||strs[strs.length-1]=="mp4")
									{
										document.write('<script type="text/javascript" src="/player/player/js/swfobject.js"><\/script>\n\r');
										document.write('<div class="video" id="CuPlayer"><b><img src="/player/player/images/loading.gif" /> 网页视频播放器加载中，请稍后...</b></div>\n\r');
										document.write('<script type="text/javascript">\n\r');
										document.write('var so = new SWFObject("/player/player/player.swf","myCuPlayer","700","420","9","#000000");\n\r');
										document.write('so.addParam("allowfullscreen","true");\n\r');
										document.write('so.addParam("allowscriptaccess","always");\n\r');
										document.write('so.addParam("wmode","opaque");\n\r');
										document.write('so.addParam("quality","high");\n\r');
										document.write('so.addParam("salign","lt");\n\r');
										document.write('//播放器配置文件-----------------------------\n\r');
										document.write('so.addVariable("JcScpFile","/player/player/CuSunV3set.xml"); //配置文件地址 \n\r');
										document.write('//视频文件及略缩图--------------------------\n\r');
										document.write('so.addVariable("JcScpVideoPath",""); //视频地址\n\r');
										document.write('so.addVariable("JcScpImg","'+xgtp+'"); //视频图片\n\r');
										document.write('so.addVariable("JcScpAutoPlay","yes"); //是否自动播放\n\r');
										document.write('so.addVariable("JcScpStarTime","0"); //起始时间点(暂未启用)\n\r');
										document.write('so.addVariable("JcScpEndTime","0"); //结束时间点\n\r');
										document.write('so.addVariable("JcScpCuePointInfo",""); //提示点信息\n\r');
										document.write('so.addVariable("JcScpCuePointTime",""); //提示点秒数值\n\r');
										document.write('//-前置Flash广告-----------------------------\n\r');
										document.write('so.addVariable("ShowJcScpAFront","no"); //是否显示前置广告\n\r');
										document.write('so.addVariable("JcScpCountDownsPosition","top-left"); //倒计时位置\n\r');
										document.write('so.addVariable("JcScpCountDowns","5"); //广告倒计时位置\n\r');
										document.write('so.addVariable("JcScpAFrontW","700"); //前置广告宽度\n\r');
										document.write('so.addVariable("JcScpAFrontH","420"); //前置广告宽度\n\r');
										document.write('so.addVariable("JcScpAFrontPath",""); //前置广告地址\n\r');
										document.write('so.addVariable("JcScpAFrontLink",""); //前置广告链接\n\r');
										document.write('//-视频广告参数-----------------------------\n\r');
										document.write('so.addVariable("ShowJcScpAVideo","no"); //是否显示前置视频广告\n\r');
										document.write('//so.addVariable("JcScpAVideoServer",""); //前置视频广告服务器\n\r');
										document.write('so.addVariable("JcScpAVideoPath",""); //前置视频广告地址\n\r');
										document.write('so.addVariable("JcScpAVideoLink",""); //前置视频广告链接\n\r');
										document.write('//-暂停广告参数-----------------------------\n\r');
										document.write('so.addVariable("ShowJcScpAPause","no"); //是否显示暂停广告\n\r');
										document.write('so.addVariable("JcScpAPauseW","300"); //暂停广告地址\n\r');
										document.write('so.addVariable("JcScpAPauseH","250"); //暂停广告高度\n\r');
										document.write('so.addVariable("JcScpAPausePath",""); //暂停广告地址\n\r');
										document.write('so.addVariable("JcScpAPauseLink",""); //暂停广告链接\n\r');
										document.write('//-角标广告参数-----------------------------\n\r');
										document.write('so.addVariable("ShowJcScpACorner","no"); //是否显示角标广告\n\r');
										document.write('so.addVariable("JcScpACornerW","85"); //角标广告宽度\n\r');
										document.write('so.addVariable("JcScpACornerH","50"); //角标广告高度\n\r');
										document.write('so.addVariable("JcScpACornerPath",""); //角标广告地址\n\r');
										document.write('so.addVariable("JcScpACornerPosition","bottom-right"); //角标广告位置\n\r');
										document.write('so.addVariable("JcScpACornerLink",""); //角标广告链接\n\r');
										document.write('//-后置广告参数-----------------------------\n\r');
										document.write('so.addVariable("ShowJcScpAEnd","no"); //是否显示后置广告\n\r');
										document.write('so.addVariable("JcScpAEndW","400"); //后置广告宽度\n\r');
										document.write('so.addVariable("JcScpAEndH","300"); //后置广告高度\n\r');
										document.write('so.addVariable("JcScpAEndPath",""); //后置广告地址\n\r');
										document.write('so.addVariable("JcScpAEndLink",""); //后置广告链接\n\r');
										document.write('//-滚动文字广告参数-----------------------------\n\r');
										document.write('so.addVariable("ShowJcScpAMoveText","no"); //是否滚动文字广告\n\r');
										document.write('//-----------------------------------------\n\r');
										document.write('so.addVariable("JcScpSharetitle","安龙县普坪镇2018年密集烤房项目建设工程邀请合作公告"); //视频标题信息\n\r');
										document.write('so.write("CuPlayer");\n\r');
										document.write('<\/script>\n\r');
										document.write('<script language=javascript src="/player/player/js/jquery-1.4.2.min.js" type=text/javascript><\/script>\n\r');
										document.write('<script language=javascript src="/player/player/js/action.js" type=text/javascript><\/script>\n\r');
									}
								}
							</script>
						</div>
					</p>
					<!--视频结束-->
					<p style="margin-right:43px"><span style="font-family: 仿宋_GB2312; font-size: 21px;">&nbsp; </span><span style="font-family: 仿宋_GB2312; font-size: 21px;">&nbsp; &nbsp;2018年，安龙县普坪镇拟新（扩）建密集烤房10座（具体附后），根据中烟办【2012】313号《中国烟草总公司关于印发烟草企业采购管理规定的通知》、中烟黔计【2013】15号《中国烟草总公司贵州省公司关于印发采购管理实施细则的通知》、中烟黔办【2016】11号《中国烟草总公司贵州省公司关于转发中国烟草总公司三个保障机制文件的通知》等相关规定，特向社会公开邀请有房屋建筑等相关资质的施工单位参加竞争性谈判，请符合条件的有意向参与建设得施工单位于2018年4月1日前到安龙县普坪镇人民政府报名、领取资料、递交参竞相关资料、逾期不再受理。</span></p><p style="margin-right:43px;text-indent:85px"><br/></p><p style="margin-right:43px;text-indent:85px"><span style="font-family: 仿宋_GB2312; font-size: 21px;">特此公告</span></p><p style="margin-right:64px;margin-left:172px;text-indent:21px;text-align:center"><br/></p><p style="margin-right:43px;text-align:right"><span style="font-family: 仿宋_GB2312; font-size: 21px;">安龙县普坪镇人民政府</span></p><p style="margin-right:43px;text-align:right"><span style="font-family: 仿宋_GB2312; font-size: 21px;">&nbsp;2018年3月29日 &nbsp;</span></p><p style="margin-right:43px;text-align:right"><br/></p><p style="margin-right:43px"><span style="font-family: 仿宋_GB2312; font-size: 21px;">地 &nbsp; &nbsp;点：安龙县普坪镇人民政府</span></p><p style="margin-right:43px"><span style="font-family: 仿宋_GB2312; font-size: 21px;">联系电话：胡刚手机：13885930799</span></p><p style="margin-right:43px;text-indent:107px"><span style="font-family: 仿宋_GB2312; font-size: 21px;">郭菲联系电话：08595510001</span></p><p><br/></p>
