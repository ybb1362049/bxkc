	<script type="text/javascript">
		function GetQueryString(name) {
		   var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)","i");
		   var r = window.location.search.substr(1).match(reg);
		   if (r!=null) return (r[2]); return null;
		}
	</script>
	<script type="text/javascript">
		var keyValue="";
		var idxIDValue="";
		var ChannelIDValue=jQuery("#channelid").val();
		$(document).ready(function() {ChannelIDValue=jQuery("#channelid").val();
			jQuery('#search_btn').click(function(){
				InitTable();
			});
		});
		//请求数据 
			function InitTable() {
				var html="";
				var getParameter="";
				keyValue=$.trim(encodeURI(jQuery("#title").val()));
				idxIDValue=jQuery("#indexID").val();
				getParameter+="Channelid=69155";
				getParameter+="&key="+keyValue;
				if(idxIDValue.length>0)
				{
					getParameter+="&idxID="+idxIDValue;
				}
				var url='/gzdsWCMSearch/wcm4govinfo.jsp?'+getParameter+'&callback=?';//外网地址
				showloading();//加载loading
				jQuery.getJSON(url,'',function (data){
					jQuery("#searchRs").empty();//去掉loading
					if(data.rows.length>0){
						for(var i=0;i<data.rows.length;i++){
							html+="<tr>";
							var bt=data.rows[i].MetaDataTitle;
							if(bt.length>40){
								bt=bt.substring(0,40)+"...";
							}
							html+="<td width='502' align='left' style='padding:4px;height:28px;'><h1 style='position: relative;color: #333;font-weight:normal;'><a href='"+data.rows[i].docpuburl+"' target='_blank'>"+bt.replace(keyValue,'<font color=\"red\">'+keyValue+'</font>')+"</a>";
								html+="<div class='showbox'>";
								html+="<table width='100%' style='position: relative;'>";
								html+="<tr>";
								html+="<td width='50%' align='left' style='border-right: 1px solid #dddddd;'>索引号:"+data.rows[i].idxID+"&nbsp;&nbsp;</td>";
								html+="<td align='left'>文号:"+data.rows[i].fileNum+"&nbsp;&nbsp;</td>";
								html+="</tr>";
								html+="<tr>";
								if(data.rows[i].Publisher!=''){
									html+="<td colspan='2' align='left'>发布机构:"+data.rows[i].Publisher+"&nbsp;&nbsp;</td>";
								}else{
									var str_2 = "县政府办公室";
									html+="<td colspan='2' align='left'>发布机构:"+str_2+"&nbsp;&nbsp;</td>";
								}
								html+="</tr>";
								html+="<tr>";
								html+="<td colspan='2' align='left'>名称:"+data.rows[i].MetaDataTitle+"&nbsp;&nbsp;</td>";
								html+="</tr>";
								html+="<tr>";
								html+="<td width='50%' align='left' style='border-right: 1px solid #dddddd;'>生成日期:"+data.rows[i].PubDate.substring(0,10).replace(".","-").replace(".","-")+"&nbsp;&nbsp;</td>";
								html+="<td align='left'>是否有效:";
								var  isok=data.rows[i].isok;
								if(isok=='0'){
									html+="&emsp;否";
								}
								else{
									html+="&emsp;是";
								}
								html+="</td>";
								html+="</tr>";
								html+="</table>";
								html+="</div>";
							html+="</h1></td>";
							html+="<td align='center' width='114' style='border-left:#e1e1e1 1px solid'>"+data.rows[i].PubDate.substring(0,10).replace(".","-").replace(".","-")+"</td>";
							if(data.rows[i].Publisher!=''){
								html+="<td align='center' width='168' style='border-left:#e1e1e1 1px solid'>"+data.rows[i].Publisher+"</td>";
							}else{
								var str_1 = "政府办";
								html+="<td align='center' width='168' style='border-left:#e1e1e1 1px solid'>"+str_1+"</td>";
							}
							html+="</tr>";
						}
					}else{
							html+="<tr>";
							html+="<td colspan='3' height='30' align='center'>没有查询到相关记录，请核实你输入的信息是否正确!</td>";
							html+="</tr>";
					}
					$("#idData").empty();
					$("#data").hide();
					$("#page").hide();
					$("#idData").append(html);
					$(".tab_list tr:nth-child(2n)").css("background-color","#F9F8F3");
					goPage(1,20);
				});
			}
			function showloading(){ //加载loading
				 jQuery("#searchRs").html('<div class="searching">数据加载中<br/><img src="/images/loading.gif" /><div>');
			} 
	</script>
	<script type="text/javascript">
		function goPage(pno,psize){
			var itable = document.getElementById("idData");
			var num = itable.rows.length;//表格所有行数(所有记录数)
			console.log(num);
			var totalPage = 0;//总页数
			var pageSize = psize;//每页显示行数
			//总共分几页 
			if(num/pageSize > parseInt(num/pageSize)){   
					totalPage=parseInt(num/pageSize)+1;   
			   }else{   
				   totalPage=parseInt(num/pageSize);   
			   }   
			var currentPage = pno;//当前页数
			var startRow = (currentPage - 1) * pageSize+1;//开始显示的行  31 
			   var endRow = currentPage * pageSize;//结束显示的行   40
			   endRow = (endRow > num)? num : endRow;    40
			   console.log(endRow);
			   //遍历显示数据实现分页
			var tempStr="";
			for(var i=1;i<(num+1);i++){    
				var irow = itable.rows[i-1];
				if(i>=startRow && i<=endRow){
					irow.style.display = "block";
					
				}else{
					irow.style.display = "none";
				}
			}
			var pageEnd = document.getElementById("pageEnd");
//			var tempStr = "共<font color=red>"+num+"</font>条记录 共<font color=red>"+totalPage+"</font>页 当前第<font color=red>"+currentPage+"</font>页";
			var tempStr="";
			if(currentPage>1){
				tempStr += "&nbsp;<a href=\"javascript:;\" onClick=\"goPage("+(1)+","+psize+")\">首页</a>";
				tempStr += "&nbsp;<a href=\"javascript:;\" onClick=\"goPage("+(currentPage-1)+","+psize+")\">上一页</a>"
			}else{
				tempStr += "&nbsp;<a href=\"javascript:;\">首页</a>";
				tempStr += "&nbsp;<a href=\"javascript:;\">上一页</a>";    
			}
		
			if(currentPage<totalPage){
				tempStr += "&nbsp;<a href=\"javascript:;\" onClick=\"goPage("+(currentPage+1)+","+psize+")\">下一页</a>";
				tempStr += "&nbsp;<a href=\"javascript:;\" onClick=\"goPage("+(totalPage)+","+psize+")\">尾页</a>";
			}else{
				tempStr += "&nbsp;<a href=\"javascript:;\">下一页</a>";
				tempStr += "&nbsp;<a href=\"javascript:;\">尾页</a>";    
			}
		
			document.getElementById("barcon").innerHTML = tempStr;
			
		}
	</script>
</head>
