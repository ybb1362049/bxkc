window.gPage = window.parent.gPage? window.parent.gPage:{};
window.gPage.appId = window.gPage.appId? window.gPage.appId :'';
window.setGPage = function(page){
	if(page){
		window.gPage.menuId = page.menuId ? page.menuId :window.gPage.menuId;
		window.gPage.pageId = page.pageId ? page.pageId :window.gPage.pageId;
	}
};
window.setAndGetGPage = function(page){
	window.setGPage(page);
	if(window.gPage){
		return 'access='+JSON.stringify(window.gPage);
	}
};
window.GetJsonGPage = function(page){
	window.setGPage(page);
	if(window.gPage){
		return {access:JSON.stringify(window.gPage)};
	}
};
seajs.use('common/priviledge.js?v=20170720', function(priviledge){
	priviledge.initButtons('', []);
	priviledge.globalSetting();
});
seajs.use('common/nav.backspace.prevent.js?v=20170720');
window.ctx = '/gap';
</script>
