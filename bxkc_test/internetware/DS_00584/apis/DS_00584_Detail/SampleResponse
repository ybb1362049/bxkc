function CheckSearchData(flag){
	if(document.searchForm.SearchText.value==""||document.searchForm.SearchText.value=="'"){
		alert("搜索时关键字不能为空！")
		document.searchForm.SearchText.focus();
		return false;
	}
	location.href="/htm/list__" + document.searchForm.searchtype.value + "_" + document.searchForm.SearchText.value + "_1.html";
}

function showtopmenu(divId,zDivCount,class1,class2){ 
for(i=0;i<=zDivCount;i++){
	document.getElementById("syqhdiv"+i).style.display="none";
	document.getElementById("syqh"+i).className=class1;
}
document.getElementById("syqhdiv"+divId).style.display="block"; 
document.getElementById("syqh"+divId).className=class2;
}
</script>
