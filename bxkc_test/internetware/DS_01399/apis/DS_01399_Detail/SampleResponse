<div class="box02" id="male">
	<form method="post" name="searchForm" action="" target="_search" style="margin:0;">
	<input type="text" name="sw" value="输入标题或名称" class="input01" onfocus="javascipt:doOnfocus(this);" onblur="javascript:doOnblur(this);"/>
	<input type="radio" id="xx" checked="checked" name="st" value="info" />
	<label name="xx" class="checked" for="xx">信息</label>
	<input type="radio" id="jg" name="st" value="org" />
	<label name="jg" for="jg">机构</label>
	<input type="button" name="input" class="input03" value="查询" onclick="javascript:search();" />
	</form>
</div>
<script type="text/javascript">
	window.onload = function () {
		labels = document.getElementById('male').getElementsByTagName('label');
		radios = document.getElementById('male').getElementsByTagName('input');
		for (i = 0, j = labels.length; i < j; i++) {
			labels[i].onclick = function () {
				if (this.className == '') {
					for (k = 0, l = labels.length; k < l; k++) {
						labels[k].className = '';
						radios[k].checked = false;
					}
					this.className = 'checked';
					try {
						document.getElementById(this.name).checked = true;
					} catch (e) {}
				}
			}
		}
	}
	
	function search() {
		if (searchForm.sw.value == '' || searchForm.sw.value == '输入标题或名称') {
			searchForm.sw.focus();
			alert("输入标题或名称。");
		}
		else {
			if (searchForm.st[0].checked == true) searchForm.action = 'searchInfo.action';
			else searchForm.action = 'searchOrg.action';
			searchForm.submit();
		}
	}
	
	function doOnfocus(e) {
		if (e.value == '输入标题或名称') e.value = '';
	}
	
	function doOnblur(e) {
		if (e.value == '') e.value = '输入标题或名称';
	}
</script>
