<script type="text/javascript">
function searchInfo(){
    var q = $('input[name=q]').val();
    if(!q || q == "项目编号/项目名称的关键词"){
        alert("请输入搜索关键词！");
        return false;
    }

    $.post('/index/searchinfo.shtml', {q:q}, function(rsp){
        if(rsp.status){
            window.location.href = '/deal/'+rsp.data.bidid+'.shtml';
        }else{
            alert(rsp.msg);
            return false;
        }
    });
}
</script>
