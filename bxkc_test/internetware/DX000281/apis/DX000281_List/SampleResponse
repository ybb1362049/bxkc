function checkForm(){
	//去掉录入框中两边的空格
	document.getElementById("userCode").value = document.getElementById("userCode").value;
	document.getElementById("password").value = document.getElementById("password").value;
	document.getElementById("rand").value = document.getElementById("rand").value;
	if(document.getElementById("userCode").value.length == 0){
		alert("请输入登录名");
		document.getElementById("userCode").focus();
		return false;
	}
	if(document.getElementById("password").value.length == 0){
		alert("请输入密码");
		document.getElementById("password").focus();
		return false;
	}
	if(document.getElementById("rand").value.length == 0){
		alert("请输入验证码");
		document.getElementById("rand").focus();
		return false;
	}
    alert(document.getElementById("rand").value);

	document.loginform.submit();
    
	}

