<script language="javascript">
var maxPageCount = 191;
function setForm(str){
	document.getElementById("page.intPage").value=str;
	//document.getElementById("page.pageInfoCount").value=document.fenyeForm.selectPageInfoCount.value;
}
function checkGoPage(){
	//验证页数不允许为空
	if(document.fenyeForm.goPage.value.length==0){
		alert("请输入要跳转的页数!");
		return false
	}
	//验证输入页数必须是数字
	if(!checkNUM(document.fenyeForm.goPage.value)){
		alert("跳转页数必须为数字!");
		return false
	}
	//验证页数不能大于最大页数
	if(document.fenyeForm.goPage.value>maxPageCount){
		alert("超出最大页数!");
		return false
	}
	setForm(document.fenyeForm.goPage.value);
	formSubmit();
}
//1.js验证只能输入数字.    
function checkNUM(value){    
	//定义正则表达式部分    
	var reg = /^\d+$/;    
	if( value.constructor === String ){    
		var re = value.match( reg );    
		if(re != null)
			return true;
	}
	return false;
}
</script> 
</form>
</div>
</div>
</div>
</div>


