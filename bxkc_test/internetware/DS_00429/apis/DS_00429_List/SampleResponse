	    /*searchStart*/
   function search_Web()
		{
			var tim=10
			if(tim==10)
			{
				tim=0;
				if (document.getElementById("txt_Search").value == "" || document.getElementById("txt_Search").value == "请输入关键字搜索") {
            }
            else {
                var t = document.getElementById("txt_Search").value;
                if (t.indexOf("<") > -1 || t.indexOf(">") > -1) {
                    alert("请勿在关键字中输入非法字符!");
                    return;
                }
				var cate="002001001001";
				var openUrl = '/laztb/showinfo/searchresult.aspx?EpStr3=';
				openUrl += escape(t)+'&searchtype=title&categorynum='+cate;
				window.location.href=openUrl;
			}
			}
			else
		    {
				alert("两次搜索的时间间隔不能少于十秒");
		    }
		}
/*searchEnd*/
	</script>
