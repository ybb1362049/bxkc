<script type="text/javascript">
    
    // 分页查询评论
    function pager(pagenum) {
    	show(pagenum);
    }
   
    // 显示评论数据的方法
    function show(pagenum) {
    	$.ajax({
        url: '/qnwebapp/comment/listComment?pageNum=' + pagenum + '&pageSize=5&idVal=' + $("#chanDocId").val(),
        dataType:'json',
       success: function(datas) {
            var data;
            if(typeof datas == 'string') {
              data = $.parseJSON(datas);
            }
        	var comments = data.topics;
              
        	var str = "";
            var itmes = '';
          for(var i in comments) {	
          	str += '<div class="zfnr">';
            str += '<div class="zfnr_1"><em>'+ (comments[i].subUser == "guest" ? "匿名用户" : comments[i].subUser) + ':</em></div>';
            str += '<div class="zfnr_2">';
            str += '<p>'+ comments[i].subContent +'</p>';
            str += '</div><div class="zfnr_1"><span>发表于' + comments[i].subTimeString + '</span></div> </div>';
          }
			

            str += "<div id='pager' class='pager_num'  style='display: block;float:none;'></div>";
      		
      		
            $("#comment").html(str); 

      		// 分页信息
      var pager_str = "页次:<span style='color:red;'>" + data.pageNumber + "/" + data.pageCount + "</span>页&nbsp;每页<span style='color:red;'>" + data.pageSize +"</span>条&nbsp;共<span style='color:red;'>" + data.totalCount + "</span>条";
      pager_str += "&nbsp;&nbsp;<a href='javascript:void(0)' onclick='pager(1)'>首页</a>&nbsp;";
      if(data.hasPrePage) {
      	pager_str += "&nbsp;&nbsp;<a href='javascript:void(0)' onclick='pager(" + data.prePage + ")'>上一页</a>&nbsp;";
      }else {
      	pager_str += "&nbsp;&nbsp;<a href='javascript:void(0)' disabled='disabled' onclick='pager(" + data.prePage + ")'>上一页</a>&nbsp;";
      }
      if(data.hasNextPage) {
      	 pager_str += "&nbsp;&nbsp;<a href='javascript:void(0)' onclick='pager(" + data.nextPage + ")'>下一页</a>&nbsp;";
      } else {
      	pager_str += "&nbsp;&nbsp;<a href='javascript:void(0)' disabled='disabled' onclick='pager(" + data.nextPage + ")'>下一页</a>&nbsp;";
      }
      pager_str += "&nbsp;&nbsp;<a href='javascript:void(0)' onclick='pager(" + data.pageCount + ")'>尾页</a>&nbsp;";   
      var n = data.pageCount;
      pager_str +='&nbsp;&nbsp;跳转到';
      pager_str += "&nbsp;&nbsp;<select id='selectpage' onchange='pager($(this).val())'>";
      for(var i = 1; i <= n; i++) {
        if(data.pageNumber == i) {
          pager_str += "<option value='" + i + "' selected>第" + i + "页</option>";
        } else {
          pager_str += "<option value='" + i + "'>第" + i + "页</option>";
        }        
      }
      pager_str += "</select>";
      		$("#pager").html(pager_str);
        }
      });
    }
    $(function() {
      // 显示评论信息
      show(1);
      
      // 清空表单
      $("#resetBtn").click(function() {
      	$("#_form")[0].reset();
      });
   
    });
 
   //验证码开始
  $(function() {
	    $.validCode = function(target, validId) {
		$.ajax({
        url : '/qnwebapp/member/getValidId?random=' + Math.random(),
				async : false,
				dataType : 'json',
				success : function(data) {
					var PageUrl = '/qnwebapp/ImageServlet?validId='
					+ data.validId;
					target.attr('src', PageUrl);
					target.css('display', '');
					validId.val(data.validId);
				},
				error : function(XMLHttpRequest, textStatus,errorThrown) {
					var msg = XMLHttpRequest.responseText;
					alert(msg);
				}
				});
	}
	$.validCode($('#imgValid'), $('#validId'));
	$('#imgValid').click(function() {
		$.validCode($('#imgValid'), $('#validId'));
	});
   //验证码结束 

 });    

  // 保存评论
  function saveComment() {
    var anonymity = $("input[name='anonymity']:checked").val();
    var subUser = "";
    if(anonymity == 'yes'){
    	subUser = "匿名网友";
    }else{
    	subUser = $("#subUser").val() ? $("#subUser").val() : '匿名网友';
    }
    
    var chanDocId = $("#chanDocId").val(); // 获取栏目文章的id
    var subContent = $("#subContent").val(); // 获取评论的内容

    if($.trim(subContent) == "") {
    	alert("请输入评论内容");
      return;
    }
    
    var code = $("#code").val();
    if($.trim(code) == "") {
    	alert("请输入验证码");
      return ;
    }
    
    $.ajax({
      url : "/qnwebapp/member/checkCode",
        data : "validId=" + $('#validId').val() + "&code="
        + $('#code').val(),
        dataType : "json",
      	async:false,
        success : function(data) {
        var success = data.success;
      //验证码验证成功
        if (success) {
            $.ajax({
            url: '/qnwebapp/comment/save',
            type: 'post',
            dataType: 'json',
            data : {
              idVal: chanDocId,
              commentTypeId: 1,
              filterFlag: 'true',
              subContent: subContent,
        	 subUser: subUser
            },
            success:function(data, textStatus) {
	   				 alert("系统提示: 留言成功!");
	    			 location.replace(location.href);
            },
             error: function(XMLHttpRequest, textStatus, errorThrown) {
               var msg = XMLHttpRequest.responseText;
               alert(msg);
             }
          });
              
      }else {
           alert("验证码错误");
    			return ;	
    }
    },
      error : function(XMLHttpRequest, textStatus,
                       errorThrown) {
        var msg = XMLHttpRequest.responseText;
        alert(msg);
        return ;
      }
    });

  }
  
  </script>
