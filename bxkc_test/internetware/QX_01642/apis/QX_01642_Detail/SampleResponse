<script type="text/javascript">
$(function(){  
  
    var doc_id="883,432";
    doc_id=doc_id.replace(/,/g,"");
	$.ajax({
	    dataType: "json",
       	url: "/qnwebapp/document/getPrevAndNextDocuments",
	    data: {
	        id: doc_id
	    },
	    success: function(datas) {
          
           var result;
            if(typeof datas == 'string') {
              result = $.parseJSON(datas); 
            } else {
              result = datas;
            }
          if (!result || typeof result != 'object') return;
	    	var prev, next, prevHtml, nextHtml;
             prev=result.prev;
          	 next=result.next;
	    	if(prev) {
          
	    	   prevHtml = '<a href="' + prev.url + '" title="' + prev.title + '">' + prev.title + '</a>';
	    	}else{
              
              prevHtml = '<a>没有了</a>';
                }
	    	if(next) {
	    		nextHtml = '<a href="' + next.url + '" title="' + next.title + '">' + next.title + '</a>';
	    	}else{
              nextHtml = '<a>没有了</a>';
                }
          
	    	if(prevHtml && nextHtml) {
              $('#preAndNext').html('<p><span>·【上一篇】：</span>' + prevHtml + '</p><p><span>·【下一篇】：</span>' + nextHtml +'</p>');
	    	} else if(prevHtml) {
              $('#preAndNext').html('<p><span>·【上一篇】：</span>' + prevHtml+'</p>');
	    	} else if(nextHtml) {
	    		$('#preAndNext').html('<p><span>·【下一篇】：</span>' + nextHtml +'</p>');
	    	}
	    },
      error:function(){
    	 $('#preAndNext').html('<p><span>·【上一篇】：</span><a>没有了</a></p><p><span>·【下一篇】：</span><a>没有了</a></p>');
    	}
	});
});

$(function(){
	var doc_id="883,432";
    doc_id=doc_id.replace(/,/g,"");
    var keywords="";
      $.ajax({
     	dataType: "json",
       	url: "/qnwebapp/document/RelationDocument",
        type:'POST',
	    data: {
        	site_id: '1',
	        doc_Id: doc_id,
            keywords : keywords
	    },
        success: function(datas) {
        	var result;
            if(typeof datas == 'string') {
              result = $.parseJSON(datas); 
            } else {
              result = datas;
            }
            var accList = result.datas;
            var html='<ul>';
            if(accList != null && accList != ''){
              for(var i=0; i<accList.length; i++){
               	var titles = accList[i].title;
                if(titles.length > 120){
                   titles = titles.substring(0,120);
                }
                 if(i<5){
                  html += '<em></em><em></em><li><a title="'+accList[i].title+'" href="'+accList[i].url+'">'+titles+'</a></li>';
                 }
              }
              
            }
            html += '</ul>';
            $("#documents").html(html);
        }
    });
});
</script>   
    
