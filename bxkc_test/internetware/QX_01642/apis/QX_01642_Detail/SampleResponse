      function OpenSelect(optionStr){
        if(optionStr != ""){
          //window.open(optionStr,'_blank')
          if(window.confirm('您访问的链接即将离开“您将离开长顺县人民政府网站”，是否继续？')){
               //alert("确定");
               //return true;
               window.open(optionStr,'_blank');
          }else{
               //alert("取消");
               return false;
          }
        }  
      }  
