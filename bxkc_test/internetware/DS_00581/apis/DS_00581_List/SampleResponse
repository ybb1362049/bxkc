    <script type="text/javascript">
        //            $(function () {
        //                $("#slides").slidesjs({
        //                    width: 334,
        //                    height: 237
        //                });
        //            });

        $(function () {
            $.ajax({
                type: "GET",
                contentType: "application/json",
                url: ("/Handler/Project.ashx?Flag=GetBidNoticeProjectState&noticeids=" + noticeArray.join(",")),
                success: function (data) {
                    var a = data.split(",");
                    var info_width = 0;
                    var info_id = 0;
                    for (var i = 0; i < a.length; i++) {
                        var b = a[i].split("-");

                        if (b.length > 1) {
                            info_id = b[0];
                            var info_width_b = b[1].toString().length;
                            if (info_width < info_width_b) {
                                info_width = info_width_b;
                                //info_width = info_width * 12;
                            }

                            $("#" + b[0] + "_INFO").html(b[1]);
                            $("#" + b[0] + "_INFO").attr("title", b[1]).addClass("text-ellipsis");
                        }
                    }
                    if (info_width > 15)
                        info_width = 15;

                    $("#" + info_id + "_INFO").parents("ul:eq(0)").addClass("zt" + info_id);
                    $('#projectstatusstyle').append(".zt" + info_id + " .zt-title{width:" + (info_width * 12) + "px;}");

                },
                error: function (e) {
                    js.screen(false);
                }
            });

            $.ajax({
                type: "GET",
                contentType: "application/json",
                url: ("/Handler/Project.ashx?Flag=GetBidNoticeCorrectionNotice&noticeids=" + noticeArray.join(",")),
                success: function (data) {
                    var a = data.split(",");
                    var info_width = 0;
                    var info_id = 0;
                    for (var i = 0; i < a.length; i++) {
                        var b = a[i].split("-");

                        if (b.length > 1) {
                            info_id = b[0];
                            info_width = ('更正公告'.length) * 12;
                            $("#" + b[0] + "_INFO_GZ").html('<a target="_blank" style="color:#2A61CB;" href="/Content/' + b[1] + '_1/' + b[2] + '">更正公告</a>');
                        }
                    }

                    $("#" + info_id + "_INFO_GZ").parents("ul:eq(0)").addClass("gz" + info_id);
                    $('#projectstatusstyle').append(".gz" + info_id + " .gz-title{width:" + info_width + "px;}");

                },
                error: function (e) {
                    js.screen(false);
                }
            });
        }) 
        </script>

        
