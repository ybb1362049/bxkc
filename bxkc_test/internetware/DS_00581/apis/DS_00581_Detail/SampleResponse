                        $.ajax({
                            type: "GET",
                            contentType: "application/json",
                            url: ("/Handler/Project.ashx?Flag=GetBidNoticeCorrectionNotice&noticeids=" + 23995),
                            success: function (data) {
                                var a = data.split(",");
                                for (var i = 0; i < a.length; i++) {
                                    var b = a[i].split("-");

                                    if (b.length > 1) {
                                        $("#" + b[0] + "_INFO_GZ").html('<a target="_blank" style="color:#2A61CB;" href="/Content/' + b[1] + '_1/' + b[2] + '">查看更正公告</a>');
                                    }
                                }

                            },
                            error: function (e) {
                                js.screen(false);
                            }
                        });
