    <script type="text/javascript">
        function focus_text() {
            var t = $(this), tp = t.parent().parent();
            tp.addClass("nl-forms-edit").addClass("nl-forms-focus");
        }
        function blur_text() {
            var t = $(this), tp = t.parent().parent();
            tp.removeClass("nl-forms-focus");
            if (t.val().trim().length == 0)
                tp.removeClass("nl-forms-edit");
        }
        $(function () {
            $("#txtbsn,#txtygzh,#txtpass").keydown(function () {
                if (event.keyCode == 13) {
                    var _txtBossName = $("#txtbsn").val(), _txtUserName = $("#txtygzh").val(), _txtUserPwd = $("#txtpass").val(), _code = $("#txtyzm").val();
                    if ($.trim(_txtBossName) == "") {
                        js.alert("BOSS号（单位账号）不能为空。", 5000, 1);
                    }
                    else if ($.trim(_txtUserName) == "") {
                        js.alert("员工账号不能为空。", 5000, 1);
                    }
                    else if ($.trim(_txtUserPwd) == "") {
                        js.alert("登录密码不能为空。", 5000, 1);
                    }
                    else
                        Login();
                }
            });
            $("#h_title a").click(function () {
                $(this).parent().children().removeClass("active");
                $(this).addClass("active");
            });
            $("#nav_login").find(".text").focus(focus_text).blur(blur_text);
        });
            var refurl = "/";
        function Login() {
            var _isC = /#Center_Login/.test(window.location.href) == true;

            js.screen(true);
            js.alert("正在验证登录信息...", -1, 1);

            var _txtBossName = $("#txtbsn").val()
                                      , _txtUserName = $("#txtygzh").val()
                                      , _txtUserPwd = $("#txtpass").val()
                                      , _code = $("#txtyzm").val();

            js.ajaxsubmit("/Ajax/Sign.ashx?Flag=Login"
                , { loginMethod: (_isC ? "egp" : "supplier"), txtbossh: _txtBossName, txtygzh: _txtUserName, txtygmm: _txtUserPwd, code: _code, refurl: refurl, type: (_isC ? "center" : "supp") }
                , function (msg) {
                    if (/登录成功/.test(msg)) {
                        var array = msg.toString().split("|");

                        var array2 = msg.toString().split("#");
                        if (array2.length > 1) {
                            alert(array2[1]);
                        }
                        if (window.PlayProject)
                            window.PlayProject(true,msg);
                        else {
                            js.gomsg("登录成功", array[1], 1);
                        }
                    }
                    else {
                        if (window.PlayProject) { 
                            window.PlayProject(false,msg);
                        }
                        else {
                            var array = msg.toString().split("|");
                            if (array.length > 1)
                                js.gomsg(array[0], array[1], 1);
                            else
                                js.alert(msg);
                            js.screen(false); 
                        }
                    }
                    js.demindMode = "default";
                });
        }
    </script>
