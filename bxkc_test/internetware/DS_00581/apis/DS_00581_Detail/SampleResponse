<script type="text/javascript">
    var checkDown = false;
    function DownLoadCheck(check) {
        var signMark = "false" == "true";
        var signPlayProjectLink = '/Bank/BankList.aspx?MenuMark=1&mark=3&key=&djid=22542';
        if (signMark) {
            window.location.href = signPlayProjectLink;
            return false;
        }

        js.gomsg("当前未登陆，请登录后再继续操作。", "/AccountLogin.aspx?methods=center,buss,supp&type=supp&ReturnUrl=" + signPlayProjectLink.replace("?", "_s7_").replace(/\&/g, "_s7s7_"));
    }

</script>
<script type="text/javascript">
    function PlayProject() {
        $('#sign_submit_control').hide();
        js.ajaxsubmit("/Handler/Handler.ashx?Flag=DownloadDocumentsInfo", { pid: '23995' }, function (data) {
            if (/登录后再试/.test(data)) {
                $('#sign_submit_control').show();
                js.screen(false);
                return;
            }

            var msg = data.toString();
            if (msg.toString().indexOf("|") == -1) {
                js.alert(msg);
            } else {
                var array = msg.toString().split("|");
                js.gomsg(array[0], array[1]);
            }
            js.demindMode = "default";
        });
    }
</script>
