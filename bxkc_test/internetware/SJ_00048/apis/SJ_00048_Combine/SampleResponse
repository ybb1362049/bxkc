<script type="text/javascript">
function jump(no,size){
	 if(size==null || size==""){
	   document.getElementById('pageNo').value= no;
     var mhform=document.forms['queryform']
     for( var k=0 ;k<mhform.elements.length;k++){
       var ins=document.createElement("input")
       ins.type="hidden" 
        ins.name=mhform.elements[k].name
       ins.value=mhform.elements[k].value
       document.forms['page'].appendChild(ins);
     }
	  document.forms['page'].submit();
}else{
     var mhform=document.forms['queryform']
     for( var k=0 ;k<mhform.elements.length;k++){
       var ins=document.createElement("input")
       ins.type="hidden" 
        ins.name=mhform.elements[k].name
       ins.value=mhform.elements[k].value
       document.forms['page'].appendChild(ins);
     }
	   document.getElementById('pageNo').value= no;
    document.getElementById('pageSize').value= size
    document.forms['page'].submit();
	}
}
</script>
