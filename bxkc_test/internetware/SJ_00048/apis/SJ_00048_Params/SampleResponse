<script type="text/javascript">
$().ready(function(){
	//显示网站导航
    $("#tl_nav").hover(function(){
        $("#tl_nav .more_nav").show();
    });
    //隐藏网站导航
    $("#tl_nav").hover(function(){},function () {
        $("#tl_nav .more_nav").hide();
    });
    
});
//查询
function queryLike(){
	var gjz = $("#gjz").val();
	if (gjz == '' || gjz ==null) {
        $('#gjz').focus();
		alert('请填写搜索内容');
	} else {
	    document.queryform.action = "/xwzs!queryXwxxSsqx.action?xwzsPage.GJZ="+gjz;
		document.queryform.submit();
	}
}

//查询
function queryGys(){
	var gjz = $("#gysmc").val();
	if (gjz == '' || gjz ==null) {
        $('#gysmc').focus();
		alert('请填写名称');
	} else {
	    document.queryform.action = "/xwzs!queryGyscx.action";
		document.queryform.submit();
	}
}

//更换验证码图片
function queryYzm(zt){
	if(zt=='1'){
	    var ImgCodeInst = document.getElementById("randcode1");
	    var dt = new Date();
	    ImgCodeInst.src = "/zhcx!queryYzm.action?"+dt.getMilliseconds();
	}else if(zt == '2'){
		var ImgCodeInst = document.getElementById("randcode2");
	    var dt = new Date();
	    ImgCodeInst.src = "/zhcx!queryYzm.action?"+dt.getMilliseconds();
	}else{
		var ImgCodeInst = document.getElementById("randcode3");
	    var dt = new Date();
	    ImgCodeInst.src = "/zhcx!queryYzm.action?"+dt.getMilliseconds();
	}
}

function tz(dm){
	$("#zoneid_id").val(dm);
	$("#tzF").submit();
}

$(function(){
	$("div.tzgg").myScroll({
		speed:40 ,
		
		  rowHeight:187 //li的高度
		
	    
	});
});
