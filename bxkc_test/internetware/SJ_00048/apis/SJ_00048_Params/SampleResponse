<script type="text/javascript">
function onSubmitLogin(strFormId,zt){
    if(!onChkLogin(strFormId,zt)){
        return false;
    }else{
        var err="";
    	var yzm = $("#uyzm"+zt).val();
    	var yzmurl = "/zhcx!queryYzmSfzq.action";
    	var yzmparam = {"map.YZM":yzm};
    	$.ajax({
    		type: "post",
    		url: yzmurl,
    		data: yzmparam,
    		async:false,
    		success:function(res){
    			var map = res.map;
    			if(map.T == "1"){
    				err = map.T;
    			}else{
    				err = map.T;
    				alert("验证码错误,请重新输入!");
    				$("#uyzm"+zt).val("");
    				$("#uyzm"+zt).focus();
    			}
    		}
    	});
    	if(err == "1"){
        	return true;
        }else{
        	queryYzm(zt);
			return false;
        }
    }
}

function onChkLogin(strFormId,zt){
    var Letters = "0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ_.-";
    if ($("#"+strFormId+" input[name='lname']").val() == "" ){
        alert ("请输入登录名称!");
        $("#"+strFormId+" input[name='lname']").focus();
        return false;
    }
    for (i=0; i < $("#"+strFormId+" input[name='lname']").val().length; i++){
        var CheckChar = $("#"+strFormId+" input[name='lname']").val().charAt(i);
        CheckChar = CheckChar.toUpperCase();
        if (Letters.indexOf(CheckChar) == -1){
            alert ("您输入的登录名称格式有误:\n\n登录名称只允许输入数字以及英文字母或者下划线、点!");
            $("#"+strFormId+" input[name='lname']").focus();
            return false;
        }
    }

    if ($("#"+strFormId+" input[name='upass']").val() == "" ){
        alert ("请输入用户密码!");
        $("#"+strFormId+" input[name='upass']").focus();
        return false;
    }
    for (i=0; i < $("#"+strFormId+" input[name='upass']").val().length; i++){
        var CheckChar = $("#"+strFormId+" input[name='upass']").val().charAt(i);
        CheckChar = CheckChar.toUpperCase();
        if (Letters.indexOf(CheckChar) == -1){
            alert ("您输入的用户密码格式有误:\n\n用户密码只允许输入数字以及英文字母或者下划线、点!");
            $("#"+strFormId+" input[name='upass']").focus();
            return false;
        }
    }
    if ($("#uyzm"+zt).val() == "" ){
        alert ("请输入验证码!");
        $("#uyzm"+zt).focus();
        return false;
    }
    return true;
}

//下载
function downFile(fjbh){
	var sjs=$("#sjs").val();
	$("#sjs").val(sjs+1);
	location.href="/daima!download.action?sjs="+sjs+"&fjbh="+fjbh;
}

function tz1(dm){
	$("#id_id").val(dm);
	$("#tzLw").submit();
}
</script>

