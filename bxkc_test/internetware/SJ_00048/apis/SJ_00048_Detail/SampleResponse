	$().ready(function(){
	    $('#Nav1').addClass("select");
	  //显示网站导航
	    $("#tl_nav").hover(function(){
	        $("#tl_nav .more_nav").show();
	    });
	    //隐藏网站导航
	    $("#tl_nav").hover(function(){},function () {
	        $("#tl_nav .more_nav").hide();
	    });
	});
	
	//查询
	function queryLike(){
	    document.queryform.action = "/xwzs!queryXwxxSsqx.action";
		document.queryform.submit();
	}
	
	//下载
	function downFile(fjbh){
		var sjs=$("#sjs").val();
		$("#sjs").val(sjs+1);
		location.href="/daima!download.action?sjs="+sjs+"&fjbh="+fjbh;
	}
	//查询计划信息
	function onShowJhxx(jhbh,xwbh){
		$("#queryform_JHBH").val(jhbh);
		$("#queryform_XWBH").val(xwbh);
		document.ff1.action = "/cgzx_ygg!queryJh.action";
		document.ff1.target="_blank";
		document.ff1.submit();
	}
</script>


