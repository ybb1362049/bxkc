<script type="text/javascript" language="javascript">
    function openSearch(isAllSearch) {
        if (document.getElementById("KeyWord").value == "" || document.getElementById("KeyWord").value == "请输入搜索内容") {
            alert("请输入查询关键字！");
        }
        else {
            window.open('http://www.cdfgj.gov.cn' + '/News/ShowAllList.aspx?KeyWord=' + escape(document.getElementById("KeyWord").value) + "&IsAllSearch=" + isAllSearch);
        }
        return false;
    }
    function SubmitKeyClick(button) {
        if (event.keyCode == 13) {
            event.keyCode = 9;
            event.returnValue = false;
            document.getElementById(button).click();
        }
    }
</script>
<script type="text/javascript">
    var toLang=0;
    $(function(){
        toLang=0;
        $("#StranLink").text(toLang==0?'繁體中文':'简体中文');
        StranBody();
        $("#StranLink").unbind("click").bind("click", function () {
            jQuery.ajax({
                url: getBasePath()+"/AjaxHelpHandle/AjaxSetCookie.ashx",
                dataType: "json",
                async: false,
                cache: false,
                error: function () {
                
                },
                success: function (json) {
                    if(json!=null){
                        toLang=json.lang;
                        $("#StranLink").text(toLang==0?'繁體中文':'简体中文');
                        StranBody();
                    }
                }
            });
        });
    }); 
</script>

