<!--
	function trim(str){
		//用正则表达式将前后空格,用空字符串替代
		return  str.replace(/\s|　|&nbsp;/gi, "");
	}
	function checkDate(frm){
		
		frm.RUN_DATE.value = "";
		frm.RUN_DATE2.value = "";
		
		var b = false;
		var startTime = document.searchForm.RUN_DATE_Y.options[frm.RUN_DATE_Y.selectedIndex].value+"."+document.searchForm.RUN_DATE_M.options[frm.RUN_DATE_M.selectedIndex].value+"."+document.searchForm.RUN_DATE_D.options[frm.RUN_DATE_D.selectedIndex].value;
		var endTime = document.searchForm.RUN_DATE2_Y.options[frm.RUN_DATE2_Y.selectedIndex].value+"."+document.searchForm.RUN_DATE2_M.options[frm.RUN_DATE2_M.selectedIndex].value+"."+document.searchForm.RUN_DATE2_D.options[frm.RUN_DATE2_D.selectedIndex].value;
		
		//alert(startTime+"-"+endTime);
		
		if(startTime.length>2&&startTime.length!=10){
			return true;	
		}
		if(endTime.length>2&&endTime.length!=10){
			return true;	
		}
		
		if(startTime.length==10){
				frm.RUN_DATE.value = startTime;
		}
		
		if(endTime.length==10){
				frm.RUN_DATE2.value = endTime;
		}
	
		
		
		return b;
	}
	function checkSearch(frm){
		var keyword = "%";
		if(checkDate(frm)){
				alert("请正确选择日期！");
				return false;
		}
		if(frm.DOCTITLE.value!='')
		{
			keyword += " and DOCTITLE='%"+frm.DOCTITLE.value+"%'";
                   
		}
		if(frm.RUN_DATE.value!='')
		{
		
keyword += " and RUN_DATE >="+frm.RUN_DATE.value;
            

		}
		if(frm.RUN_DATE2.value!='')

		{
			
 keyword += " and RUN_DATE <="+frm.RUN_DATE2.value; 


		}
		if(frm.Aysj_endtime.selectedIndex!=0)
		{
			keyword += " and Aysj_endtime='%"+frm.Aysj_endtime.options[frm.Aysj_endtime.selectedIndex].value+"%'";
		}
		if(frm.Aysj_type.selectedIndex!=0)
		{
			keyword += " and Aysj_type='%"+frm.Aysj_type.options[frm.Aysj_type.selectedIndex].value+"%'";
		}
		if(frm.AYSJ_ZBZZXS.selectedIndex!=0)
		{
			keyword += " and AYSJ_ZBZZXS='%"+frm.AYSJ_ZBZZXS.options[frm.AYSJ_ZBZZXS.selectedIndex].value+"%'";
		}
		if(frm.AYSL_ZBFS.selectedIndex!=0)
		{
			keyword += " and AYSL_ZBFS='%"+frm.AYSL_ZBFS.options[frm.AYSL_ZBFS.selectedIndex].value+"%'";
		}
		if(frm.SUBDOCTITLE.selectedIndex!=0)
		{
			keyword += " and SUBDOCTITLE='%"+frm.SUBDOCTITLE.options[frm.SUBDOCTITLE.selectedIndex].value+"%'";
		}
		/*
		if(frm.fenlei.selectedIndex!=0)
		{
			keyword += " and fenlei='%"+frm.fenlei.options[frm.fenlei.selectedIndex].value+"%'";
		}
		*/
		if(frm.wenhao.value!='')
		{
			keyword += " and wenhao='%"+frm.wenhao.value+"%'";
		}
		if(frm.docChnlId.value!="")
		{
			keyword += " and DOCCHANNEL="+frm.docChnlId.value;
		}
		if(keyword=="%")
		{
			alert("请输入检索词");
			return false;
		}
		else
		{
			frm.searchword.value = keyword;
                          //alert(keyword);
			return true;
		}
	}

