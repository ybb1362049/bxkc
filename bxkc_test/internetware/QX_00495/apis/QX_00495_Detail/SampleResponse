  function s_click(nums){    
     var num = 0;
	 var sele=document.getElementById("select"+nums);
	 for(var i=0;i<sele.options.length;i++){
		 if(sele.options[i].selected==true){                
		 num++;         
         }   
		 }  
                 var url = sele.options[sele.selectedIndex].value;    
		 if(url!="#"){          
		 window.open(url);//这里修改打开连接方式  
		 }
		 } 
</script>
