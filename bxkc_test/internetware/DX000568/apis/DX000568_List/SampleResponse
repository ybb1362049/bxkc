    $(function(){

        var num = 1;
        $('.cy-kcxfz').click(function(){
            if(num == 0){
                
                $(this).next().stop().animate({
                    'height':'116px'
                },500);
                num = 1;
            }else{
               
                $(this).next().stop().animate({
                    'height':'0px'
                },500);
                num = 0;
            }
            
        })
    })
