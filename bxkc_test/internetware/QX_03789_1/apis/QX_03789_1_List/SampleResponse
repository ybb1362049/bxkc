<script>
timer=setInterval(tack1,4000);
var step=0;
var imgNum=$('.hd_header_con p').length;
function tack1(){
$('.block').eq(0).removeClass('block').next().addClass('block');
step++;
if(step>=imgNum){$('.hd_header_con p').eq(0).addClass('block').siblings().removeClass('block');step=0;}
/*$('.block').animate({opacity:1},500);
$('.block').siblings().animate({opacity:0},500);*/
}
