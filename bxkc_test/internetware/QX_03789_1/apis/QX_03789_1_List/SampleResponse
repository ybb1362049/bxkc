    <script language="javascript">
    function globSearch(){
       var sUrl = "http://dig.lsz.gov.cn/dig/search.action?ty=&w=false&f=&dr=true&p=1&sr=score+desc&fq=&advtime=&advrange=&q=";
       //var sUrl ="http://ww.schd.gov.cn/search/SelectList.aspx?wd=";
				var key = document.getElementById("search1016").value;
				if(key == "" || key == "请输入关键字"){
					alert("关键字不能为空!");
					return false;
				}

				//key = encodeURI(key);
				sUrl = sUrl + key + '&ext=siteid:1424562'
				window.open(encodeURI(sUrl));
    }
</script>
