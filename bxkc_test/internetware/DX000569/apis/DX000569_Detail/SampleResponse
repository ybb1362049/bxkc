	function resizeImg(){
		var main_cont_width = jQuery("#main_cont").width();
		jQuery("#contentBox").find("img").each(function(){
			var _this = jQuery(this);
			if(_this.width() > main_cont_width)	_this.width(main_cont_width - 20);
		});		
	}
