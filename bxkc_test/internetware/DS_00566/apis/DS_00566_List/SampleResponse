	function doSubmit(){
		$("#q").val($.trim($("#q").val()));
		if( $("#q").val() ){
			common.loading();
			$("#p").val("1");
			$("#search-form").submit();
		} else {
			layer.config({extend:'skin/moon/style.css'});
			layer.alert('请输入要检索的关键字!', {
			    icon: 1,
			    skin: 'layer-ext-moon' ,
			    shade: false,
			    offset: ['100px']
			}, function(index){
			    jQuery('#q').focus();
			    layer.close(index);
			})
		}
	}
</script>
