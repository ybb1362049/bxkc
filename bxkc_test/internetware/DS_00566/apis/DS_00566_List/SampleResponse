    	$(function(){
    		var serviceType = '1';
    		var noticeType = '1';
    		$("#parent-" + serviceType).removeClass("le").addClass("onle");
			$("#parent-" + serviceType).siblings().show();
			var childId = "child_"+serviceType+"_"+noticeType+"";
			$("#" + childId).children("a").css("color","red");
    		
			$(".news-lf .system_log .me-lest").each(function(index) {
                $(this).click(function(){
					$(".point").hide();
					$(".news-lf .system_log .me-lest").removeClass("onle").addClass("le");
				 	$(this).addClass("onle").removeClass("le");
				    $(this).siblings().show();
				});
            });
			$(".news-lf .system_log .point .dme-1").each(function(index1) {
                $(this).click(function(){
					$(".dme-1 a").css("color","#333");
					$(this).children("a").css("color","red");
					});
            	});
			});
