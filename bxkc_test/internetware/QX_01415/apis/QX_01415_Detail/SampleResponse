    <script type="text/javascript">
        $(document).ready(function() {
            var param = "HyZbggZfcgDetail.ashx";
            function wsjjLogin() {
                if ('False' == "True") {
                    location.href = "hyZbggWsjjDetail.aspx?sgzbbm=" + escape('5108232017000298');
                    //data; // "../hylogin.aspx?qybhdl=1&goUrl=" + escape("hyzq/hyZbggWsjjDetail.aspx?sgzbbm=" + escape('5108232017000298'));
                }
                else {
                    var lOK = TestClick();
                    if (lOK) {
                        $.post(param, { k: "keylogin", sgzbbm: '5108232017000298', hidSN_SERAL: $("#hidSN_SERAL").val(), hidDigest: $("#hidDigest").val() }, function(data) {
                            if (data.indexOf("error") == 0)
                                alert(data);
                            else
                                location.href = data;
                        });
                    }
                }
            }
            $("#divWsjjXY").dialog({
                autoOpen: false,
                modal: true,
                height: 340,
                width: 500,
                buttons: {
                    "我同意": function() {
                        $(this).dialog("close");
                        wsjjLogin();
                    },
                    "我不同意": function() {
                        $(this).dialog("close");
                    }
                }
            });

            $("#IBtnBaojia").click(function() {
                if ($("#divWsjjXY").html() != "") {
                    $("#divWsjjXY").dialog("open");
                } else {
                    wsjjLogin();
                }
                return false;
            });


        });
    </script>

