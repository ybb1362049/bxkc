<script type="text/javascript">
    function GetInstall() {
        var ET99 = document.getElementById("ET_99");
        var chPid = "540cb1ff";
        var bErr = false;
        try {
            ET99.FindToken(chPid);
            ET99.OpenToken(chPid, 1);
            alert("密匙驱动已安装,密匙号:" + ET99.GetSN());
            return true;
        }
        catch (e) {
            return false;
        }
    }


    function TestClick() {
        var ET99 = document.getElementById("ET_99");
        var chPid = "540cb1ff";
        var bErr = false;
        try {
            var tokencount = ET99.FindToken(chPid);
            if (tokencount > 1) {
                alert("请一次插入一个密匙");
                return false;
            }
        }
        catch (e) {
            alert("没有插入密匙,或密匙不正确");
            return false;
        }
        try {
            ET99.OpenToken(chPid, 1);
        }
        catch (e) {
            alert("打开密匙失败,pid");
            return false;
        }
        try {
            ET99.VerifyPIN(0, "FFFFFFFFFFFFFFFF");
        }
        catch (e) {
            alert("打开密匙失败,pin,,"+e)
            ET99.CloseToken();
            return false;
        }
        var sn = "01234567890123456";
        try {
            sn = ET99.GetSN();
        }
        catch (e) {
            alert("获取密匙序号错误");
            ET99.CloseToken();
            return false;
        }
        var sName;
        try {
            sName = ET99.Read(0, 0, 60);
        } catch (e) { alert(e); }


        try {

            var Digest = ET99.MD5HMAC(1, 'eoakmsqxpvlwvjxpwhbl', 20);
            document.all("hidDigest").value = Digest;
            document.all("hidSN_SERAL").value = sn;
            document.all("hidET99UserName").value = sName;
        }
        catch (e) {
            alert("加密错误");
            ET99.CloseToken();
            return false;
        }
        ET99.CloseToken();
        return true;
    }
    
</script>


